{ "type": "Feature", "properties": { "ID": 1378, "year": 1844, "date": "1844\/09", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Efforts to control pigs running loose in the streets triggered riotous resistance by the Irish owners.", "Source": "Billington, p.196; Cook.Armies, p.27", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1398, "year": 1845, "date": "1845\/07", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: During the great fire at New street a fireman went into a German grocery at the corner of Beaver and Pearl streets and had some refreshment. He told the proprietor to charge it to his company. When the proprietor attacked him, demanding payment, forty firemen rushed in and demolished his store.", "Source": "Costello.Our Firemen, p.183", "Type": "Firefighter; Ethnic\/National", "Rioter Classification": "Firefighter", "Target Classification": "German; Merchant", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1404, "year": 1846, "date": null, "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "Theater riot at Palmo's theater against a Scottish singer named Templeton.", "Source": "Buckly. \"To the Opera House,\" p.192", "Type": "Ethnic\/National; Theater", "Rioter Classification": "Unknown", "Target Classification": "Theater; Scottish; Actor; Singer", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Templeton", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
