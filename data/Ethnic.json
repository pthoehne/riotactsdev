{ "type": "Feature", "properties": { "ID": 2021, "year": 1861, "date": "1861\/10", "State": "California", "Sublocation": "Los Angeles", "Location": "Los Angeles", "Lat": 34.022624, "Long": -118.367045, "Description": "October: Young Francisco Cota taken from police officers and lynched. He had shot an Anglo shopkeeper and his wife.", "Source": "Blew. \"Vigilantism,\" p.23", "Type": "Racial; Law; Ethnic\/National; Religious; Vigilante", "Rioter Classification": "White; Vigilante; Anglo", "Target Classification": "Hispanic; Catholic; Prisoner; Legal; Youth", "Actions": "Prison Break; Seized Person(s); Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Francisco Cota", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -118.367045, 34.022624 ] } },
{ "type": "Feature", "properties": { "ID": 2027, "year": 1861, "date": "1861\/04", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "April: Pro-secession mobs active in attacking local targets. One group sacked the German Turner Hall because it was reported that some German turners had volunteered to go to Washington in defense of the capital. Another group attacked the German abolition paper the Wecker at Frederick near Gay street.", "Source": "Scharf.Chronicles, p.600-601", "Type": "War; Political; Ethnic\/National; Slavery; Racial", "Rioter Classification": "Pro-Confederate; Confederate; Anti-Abolitionist; Pro-Slavery", "Target Classification": "Pro-Union; Union; Press; German; Abolitionist; Anti-Slavery", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 2039, "year": 1861, "date": "1861\/08", "State": "Indiana", "Sublocation": "Shelby", "Location": "Shelby, Indiana", "Lat": 41.19475, "Long": -87.34713, "Description": "August: Fight broke out between Germans and Americans at a dance at the Turner Hall. Two Germans were stabbed; one American stabbed.", "Source": "\"Turbulent Times,\" p.83", "Type": "Ethnic\/National", "Rioter Classification": "American; German", "Target Classification": "American; German", "Actions": "Non-Lethal Violence; Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -87.34713, 41.19475 ] } },
{ "type": "Feature", "properties": { "ID": 2053, "year": 1862, "date": "1862\/09", "State": "Pennsylvania", "Sublocation": "Archibald", "Location": "Archibald, Luzerne Co., PA", "Lat": 41.506797, "Long": -75.55603, "Description": "September: Enrollment officer on armed guard and Catholic priest at 1st successfully in enrolling Irish miners. Some scuffling occurred and some women arrested. In the evening a crowd attacked the hotel with the enrollment officer in it. In the ensuing riot stones were thrown and the military fired into the crowd, fatally shooting one man and wounding several others.", "Source": "Palladino.Another Civil War, p.101", "Type": "Military; Anti-War; War; Religious; Ethnic\/National", "Rioter Classification": "Irish; Catholic; Woman; Worker; Miner", "Target Classification": "Catholic; Religious; Priest; Military; Enlistment Officer", "Actions": "Lethal Violence; Non-Lethal Violence; Threat", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.55603, 41.506797 ] } },
{ "type": "Feature", "properties": { "ID": 2058, "year": 1862, "date": "1862\/05", "State": "Illinois", "Sublocation": "Jacksonville", "Location": "Jacksonville, Illinois", "Lat": 39.7968, "Long": -89.64551, "Description": "May: Constable and six man posse enter Portuguese section - called Madiena - to recover a stolen horse. They were attacked by about 100 armed Portuguese. Later, Fry returned with white mob. But bloodshed avoided. This was referred to as the Battle of Madiera.", "Source": "Doyle.Social Order, p.129", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Portuguese; Catholic", "Target Classification": "Legal; Law Officer", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -89.64551, 39.7968 ] } },
{ "type": "Feature", "properties": { "ID": 2065, "year": 1862, "date": "1862\/07", "State": "Ohio", "Sublocation": "Cincinatti", "Location": "Cincinnati", "Lat": 39.105032, "Long": -84.517971, "Description": "July: Rioting between white and black boatmen triggered by competition over jobs and wages. Irish coworkers attacked black people, their homes (setting some on fire) and beat African Americans. Some African Americans retaliated by attacking and burning Irish houses.", "Source": "Lofton. \"Northern Labor,\" J. of N. His. 34 (1949) 257", "Type": "Racial; Labor; Ethnic\/National", "Rioter Classification": "White; Black; Worker; Irish; Boatman", "Target Classification": "White; Black; Worker; Irish; Boatman", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -84.517971, 39.105032 ] } },
