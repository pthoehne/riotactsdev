{ "type": "Feature", "properties": { "ID": 1467, "year": 1849, "date": null, "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "Summer: Killers ignited at least four fires and ambushed the Shifflers before capturing the Shiffler's carriage.", "Source": "Laurie.Fire Companies, p.81", "Type": "Firefighter; Gang", "Rioter Classification": "Firefighter; Killers; Gang", "Target Classification": "Firefighter; Shifflers; Gang", "Actions": "Seized Property; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
