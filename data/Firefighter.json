{ "type": "Feature", "properties": { "ID": 1344, "year": 1843, "date": "1843\/12", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Battle outside Black Joke's engine house at Gouverneur and Henry street, when members of Bean Soup, Old Maid, and Old Turk Engine companies attacked. Numbering 600 strong. Black Joke greeted them with guns and a cannon loaded with slugs, chains, bolts. Police arrived and arrested several Black Jokes. Subsequently the company was disbanded.", "Source": "Limpas.History, p.181-182; Buckly, p.340", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence; Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1351, "year": 1844, "date": null, "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "After a false alarm two fire companies riot and fight one month.", "Source": "Greenberg.Cause for Alarm, p.90", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 1356, "year": 1844, "date": "1844\/03", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "March: Unknown group of raiders broken into Goodwill Hose Company's headquarters, stole their engine, drove it to the outskirts and burned it.", "Source": "Feldberg.TE, p.80", "Type": "Firefighter", "Rioter Classification": "Unknown", "Target Classification": "Firefighter", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1362, "year": 1844, "date": "1844\/06", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "June: Weccacoes attempted to attack American Representative Weccacoes Fire Company's engine house. Their rivals were waiting for them and fired upon the attackers. The watch then interfered and confiscated the attackers' guns.", "Source": "\"Fire Companies,\" p.80", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence; Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1380, "year": 1844, "date": "1844\/12", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "December: Fire company fight.", "Source": "Werner, p.151;Public Ledger, December 9, 1944", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1397, "year": 1845, "date": "1845\/06", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "June: Weccacoes Hose Company members attempt to storm Franklin Fire Company's house, but watchmen interceded.", "Source": "Laurie. \"Fire Companies,\" p.75", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1398, "year": 1845, "date": "1845\/07", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: During the great fire at New street a fireman went into a German grocery at the corner of Beaver and Pearl streets and had some refreshment. He told the proprietor to charge it to his company. When the proprietor attacked him, demanding payment, forty firemen rushed in and demolished his store.", "Source": "Costello.Our Firemen, p.183", "Type": "Firefighter; Ethnic\/National", "Rioter Classification": "Firefighter", "Target Classification": "German; Merchant", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1415, "year": 1846, "date": "1846\/07", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Big fight involving fire engine companies 1, 6, 23, and 36 on return from fire. All the companies were subsequently disbanded.", "Source": "Costello.Our Firemen, p.173-174; Limpus.History, p.150", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1429, "year": 1847, "date": "1847\/05", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "May: Weccacoes Hose company captured and destroyed Franklin Fire company (??).", "Source": "Laurie. \"Fire Companies,\" p.79", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1431, "year": 1847, "date": "1847\/09", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "September: Large battle between competing fire companies. New Market and United fire companies fought against Watchman fire company for 1.5 hours along Light street from York to Larden and along Burre street from Light to Sharp street. The companies were on their way to a fire in Federal Hill. Bricks and pistols used. Some of the New Market equipment captured and thrown in Larden. Police nightwatch and fire officers quelled the riot.", "Source": "Scharf.Chronicles, p.523-524", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Seized Property; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 1433, "year": 1847, "date": "1847\/12", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "December: Fire company fight.", "Source": "Warner, p.151", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1450, "year": 1848, "date": "1848\/10", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "October: Members of the Watchman Fire Company attacked the tavern of John Appleby at the corner of Caroline and Hampstead streets, demolishing every window in his house, and wounding Appleby. They also fired pistols. Appleby fired back, wounding several, and drove the rioters off. Seven men were shot.", "Source": "Scharf.Chronicles, p.528", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Merchant; Tavern", "Actions": "Lethal Violence; Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": 7, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Appleby", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 1453, "year": 1849, "date": "1849\/07", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "June: Fire purposefully set so a gang could attack Franklin Hose company on its way to the fire. That night the Franklin Hose company retaliated, attacking the Moyamensing Hose. Shots exchanged, one man killed, 9 or 10 wounded.", "Source": "Scharf and Wescott, p.691-692", "Type": "Firefighter", "Rioter Classification": "Firefighter; Franklin Hose; Gang", "Target Classification": "Firefighter; Franklin Hose; Gang", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1457, "year": 1849, "date": "1849\/10", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "October: Post-election riot at the California House pub led to race fighting leaving several black and white people dead. Fire set to California House spread to several other buildings. Rioters fight firemen. Two firemen killed, one Irishman, and one black boy. At least nine whites and 16 blacks injured. African Americans attempted to fight back at California house and elsewhere. Militia called out to quell riot.", "Source": "Feldsburg.Philadelpha Riots, p.5", "Type": "Political; Racial; Firefighter; Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; Black; Firefighter; Youth", "Target Classification": "Irish; Catholic; Black; Firefighter; Youth", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": "4", "Recorded # Injured": 25, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
