{ "type": "Feature", "properties": { "ID": 881, "year": 1829, "date": "1829\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Two respectable young bakers were attacked and maltreated in Five Points by a gang called Forty Thieves or highbinders.", "Source": "NYGazette, April 2, 1829", "Type": "Gang", "Rioter Classification": "Gang; Highbinder; Forty Thieves", "Target Classification": "Baker; Youth", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 882, "year": 1829, "date": "1829\/04", "State": "Pennsylvania", "Sublocation": "Penn Canal", "Location": "Penn Canal", "Lat": 40.281137, "Long": -75.845769, "Description": "April: Workers use violence in strike concerning wages.", "Source": "Grinmsted. \"A L,\" p. 11, 23, 40", "Type": "Labor", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Employer", "Actions": "Strike; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.845769, 40.281137 ] } },
{ "type": "Feature", "properties": { "ID": 883, "year": 1829, "date": "1829\/04", "State": "Pennsylvania", "Sublocation": "Penn Canal", "Location": "Penn", "Lat": 40.281137, "Long": -75.845769, "Description": "April: Canal workers combine for higher wages. They not only refused to work but also used vioelnce to prevent others from working. They were put down and ringleaders arrested. Most of the strikers were foreigners.", "Source": "Niles.Weekly Register, April 11, 1829", "Type": "Labor", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Strike; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.845769, 40.281137 ] } },
{ "type": "Feature", "properties": { "ID": 884, "year": 1829, "date": "1829\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: In the forenoon Mary Ann Davis' place was invaded by a number of rowdy firemen, harassing her and others who were there.", "Source": "SC PC cases 7441 5", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Mary Ann Davis", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 885, "year": 1829, "date": "1829\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: There was a riot in Cross street. In the morning of the 19th. The mob was throwing brick bats, cutting hoses, etc.", "Source": "SC PC cases 7441 6", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 886, "year": 1829, "date": "1829\/05", "State": "Pennsylvania", "Sublocation": "Clark's Ferry", "Location": "Clark's Ferry, Pa", "Lat": 40.411313, "Long": -77.00939, "Description": "May: An Irishman refused to pay for his whiskey at a local saloon. After being kicked out he returned with a number of his countrymen to pull down the building and kill the innkeeper Martin. Finding the house secure they began pulling it down. Martin threatened to shoot them, and finally, in desperation, commenced firing. No one was killed, but several were wounded. The sheriff ordered out the posse to restore order.", "Source": "NYEP, May 9, 1829", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Merchant", "Actions": "Property Destroyed; Seized Property; Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Martin", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.00939, 40.411313 ] } },
{ "type": "Feature", "properties": { "ID": 887, "year": 1829, "date": "1829\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: 1:30 the night of the 13th 20 or 30 men raised a great noise on Broadway near White street and abused Thomas Fenton and others. Four men were indicted for riot but acquitted.", "Source": "NYC General Sessions, June 6, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Thomas Fenton", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 888, "year": 1829, "date": "1829\/05", "State": "Pennsylvania", "Sublocation": "Nanticoke", "Location": "Nanticoke, Pa", "Lat": 41.203918, "Long": -76.00345, "Description": "May: Near the town \"foreigners\" employed on the North Branch canal rioted. One man was killed, several were beaten. Some of the ringleaders escaped, others were captured.", "Source": "Niles'Weekly Register, June 20, 1829", "Type": "Ethnic\/National", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Unknown", "Actions": "Lethal Violence; Non-Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.00345, 41.203918 ] } },
{ "type": "Feature", "properties": { "ID": 889, "year": 1829, "date": "1829\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: James O'Connor was beaten outside his house on Christine street by William Rice and a number of others who were all charged with riot and A+B.", "Source": "NYC General Sessions, July 16, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "William Rice", "Persons Named as Targets": "James O'Connor", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 890, "year": 1829, "date": "1829\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: When two officers entered a house in Augustus street to levy an execution on the goods of an Irishman, \"a general turn out of his countrymen took place.\" A riot was prevented by the timely arrival of the police. The goods were removed and the officers beat a hasty retreat.", "Source": "New York Statesman, June 2, 1829", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Law Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 891, "year": 1829, "date": "1829\/07", "State": "Rhode Island", "Sublocation": "Providence", "Location": "Providence, RI", "Lat": 41.8282, "Long": -71.416533, "Description": "July: About 60 to 80 persons paraded through town starting at 2:00am with \"Tin horns, conch shells, tin pans, drums and fife.\" They awoke the whole town, demolished garden fences, cut the halyards of various flag poles, hung a number of citizens in effigy an finally burnt those effigies in a bonfire.", "Source": "NYEP, July 13, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Effigy; Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.416533, 41.8282 ] } },
{ "type": "Feature", "properties": { "ID": 892, "year": 1829, "date": "1829\/07", "State": "Pennsylvania", "Sublocation": "Erie", "Location": "Erie, Penn", "Lat": 42.133234, "Long": -80.084094, "Description": "July: When a masonic methodist preacher attempted to preach at a church in Erie, he was disrupted and forced to continue in a nearby barn.", "Source": "NYEP, July 24, 1829", "Type": "Religious; Anti-Masonic", "Rioter Classification": "Anti-Masonic", "Target Classification": "Religious; Methodist; Church; Masonic", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -80.084094, 42.133234 ] } },
{ "type": "Feature", "properties": { "ID": 893, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: The African church on 6th avenue was distubed during services by a number of boys - one of whom wore his jacked inside out. Stones were thrown and a cart ring was threatened to be ued in hitting someone. This kind of activity occurred frequently. Timothy Duffy and Michael Tweeny were indicted and convicted of riot and A+B.", "Source": "NYC General Sessions, July 14, 1829", "Type": "Racial; Religious", "Rioter Classification": "Youth", "Target Classification": "Black; Church; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Timothy Duffy; Michael Tweeny", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 894, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Allan Boupe was arrested at 9:30pm for rioting. Later that evening he stabbed a watchman. For this he was prosecuted in Gen. sessions August 13.", "Source": "NYEP, August 14, 1829", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": "Allan Boupe", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 895, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Sarah Thomas was harassed in the evening by 12 to 15 persons who came to her door, sung, dance, and made a great noise. The vestry lamp was broken and they threqa handkerchief full of frogs about her room. One has several times threatened to come to her house and throw flour about it.", "Source": "SC PC 7441, 9", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Sarah Thomas", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 896, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Religious assembly held at Christie street between Hester and Walker was disrupted by Jacob C. Cauldwell and others. Cauldwell plead guilty to riot and A+B.", "Source": "NYC General Sessions, July 16, 1829", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Jacob C. Cauldwell", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 897, "year": 1829, "date": "1829\/08", "State": "Massachusetts", "Sublocation": "New Bedford", "Location": "New Bedford", "Lat": 41.651401, "Long": -70.930255, "Description": "August: Citizens destroy a 2nd ship converted into a brothel.", "Source": "Ellis.History of New Bedford", "Type": "Moral Panic; Vigilante; Law", "Rioter Classification": "Vigilante", "Target Classification": "Brothel; Prostitute; Woman", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -70.930255, 41.651401 ] } },
{ "type": "Feature", "properties": { "ID": 898, "year": 1829, "date": "1829\/08", "State": "Pennsylvania", "Sublocation": "Chambersburg", "Location": "Chambersburg, Penn", "Lat": 39.932651, "Long": -77.663306, "Description": "August: A demonstration was made upon the market house which resulted in the prostration of the Northwest pillar. \"This building has long been an eyesore to a portion of our inhabitants, but we had not supposed, that any of them were capable of convincing their displeasure to it.\"", "Source": "NYEP, August 8, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.663306, 39.932651 ] } },
{ "type": "Feature", "properties": { "ID": 899, "year": 1829, "date": "1829\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: A number of rowdies refused to obey a watchman's order to behave about two thirty am in Amity street between Wooster and Green. The watchman was manhandled by two brothers.", "Source": "NYC General Sessions, September 11, 1829", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 900, "year": 1829, "date": "1829\/08", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "August: Several disturbances by laborers at B+O railroad. On Friday Aug. 14 one man was killed near city, several wounded. On Sunday rioters ransacked the dwelling of Thomas Elliott, one of the contractors.", "Source": "Scharf.Chronicles, p.441", "Type": "Labor", "Rioter Classification": "Worker; Railroad Worker", "Target Classification": "Contractor", "Actions": "Lethal Violence; Non-Lethal Violence; Property Destroyed", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": "Thomas Elliott", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 901, "year": 1829, "date": "1829\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: A deputy sheriff levying an execution was resisted by a number of people who mobbed him. No \"personal violence was used\" but it was necessary for the high sheriff to come to disperse the crowd. This probably occurred in Chatham street. Those inolved, and 2 or 3 were apprehended and posted $1,000 and 500 bonds, were also ivolved with charging someone with swindling and they opposed the officer because they thought he was acting illegally.", "Source": "Common Advertiser, August 20, 1829", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 902, "year": 1829, "date": "1829\/08", "State": "Massachusetts", "Sublocation": "New Bedford", "Location": "New Bedford", "Lat": 41.651401, "Long": -70.930255, "Description": "August: A riot occurred as a crowd dismantled a renowned disorderly house, the Ark, after the occupants were legally forced to vacate. While doing so the building caught fire. Whether this was intentional or not is unclear.", "Source": "NYCommon Advertiser, August 26, 1829", "Type": "Moral Panic; Law; Vigilante", "Rioter Classification": "Vigilante", "Target Classification": "Brothel; Prostitute; Woman", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -70.930255, 41.651401 ] } },
{ "type": "Feature", "properties": { "ID": 903, "year": 1829, "date": "1829\/08", "State": "Ohio", "Sublocation": "Cincinnatti", "Location": "Cincinnatti", "Lat": 39.105032, "Long": -84.517971, "Description": "August: In an attempt to enforce an ordinance driving all black people out of town, a mob attacked the dwelling houses of black people, breaking windows and on Thursday and Friday night. On Saturday night a group of black residents armed themselves. When the white crowd attacked, a gun was fired, killing Ely Herricks, a young man. Ten black and seven white people were arrested that night and the mayor, on Sunday morning, issued a proclamation calling all good citizens to assist the constituted authorities in preserving the public peace.", "Source": "NYCommon Advertiser, August 26, 1829", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Lethal Violence; Property Destroyed; Threat", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Ely Herricks", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -84.517971, 39.105032 ] } },
{ "type": "Feature", "properties": { "ID": 904, "year": 1829, "date": "1829\/09", "State": "New Jersey", "Sublocation": "Perth Amboy", "Location": "Perth Amboy, NJ", "Lat": 40.512012, "Long": -74.272032, "Description": "September: Between 400 and 500 New Yorkers and Staten Islanders raided the oyster beds off Perth Amboy. To defend their crop a posse Comitats and the militia were ordered out in Perth Amboy. Shots were exchanged, but no one was killed.", "Source": "NYEP, Septebember 14, 1829", "Type": "Economic", "Rioter Classification": "Alleged Criminal; Alleged Thief", "Target Classification": "Vigilante; Militia; Military", "Actions": "Threat; Seized Property; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.272032, 40.512012 ] } },
{ "type": "Feature", "properties": { "ID": 905, "year": 1829, "date": "1829\/09", "State": "Pennsylvania", "Sublocation": "Marrieta", "Location": "Marrieta, Penn", "Lat": 40.056648, "Long": -76.559046, "Description": "September: Riots at the Banbridge Races between canal workers and the visitors to the races. A black person was killed, another man had his leg so battered by a musket shot that it had to be amputated. Others were wounded. A court case is to follow. Those workmen involved are to be dismissed from further employment on the line.", "Source": "NYEP, September 15, 1829", "Type": "Unknown", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Black", "Actions": "Lethal Violence; Non-Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.559046, 40.056648 ] } },
{ "type": "Feature", "properties": { "ID": 906, "year": 1829, "date": "1829\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: The Ebenezer church in Elizabeth street, probably for black people, was disrupted during divine services. Some scuffle ensued. Two white men were in the church while more were outside.", "Source": "NYC General Sessions, October 13, 1829", "Type": "Racial; Religious", "Rioter Classification": "White", "Target Classification": "Black; Church; Religious", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 907, "year": 1829, "date": "1829\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Riotous meeting held at Park. Russel Clark, Cidorest (he sells porter tail) had called it and was the speaker. He favored national schools where black and white people would be taught together, a law for putting the wife on par with her husband, and a law for abolishing all debts. The crowd appeared to enjoy all this entertainment, but the police magistrate summoned Comstock, told him the meeting was riotous. It took some persuading but Comstock finally agreed to desist.", "Source": "NYEP, October 3, 1829", "Type": "Political; Law", "Rioter Classification": "Reformer", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": "Russel Clark", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 908, "year": 1829, "date": "1829\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: 20 \"grown up boys\" came to Rebecca Hilton's house on PH street, made a great noise, threatened her, and threw stones at her windows. Probably occurred during the day. Result, 4 persons indicted were convicted.", "Source": "NYC General Sessions, November 12, 1829", "Type": "Unknown", "Rioter Classification": "Youth", "Target Classification": "Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Rebecca Hilton", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 909, "year": 1829, "date": "1829\/10", "State": "New York", "Sublocation": "Troy", "Location": "Troy, NY", "Lat": 42.731883, "Long": -73.686965, "Description": "October: In the lower part of the city riot erupted when some Irish attempted to have a dance at a house. Neighbors through that this was not right and threw some stones at the windows. Duley insulted, the Irish counter-attacked with fist, clubs, and the like. Nearly 200 were engaged in the tumult. The police arrived, and with difficulty arrested 3 or 4 ringleaders and quelled the riot.", "Source": "NYEP, October 30, 1829", "Type": "Ethnic\/National; Religious", "Rioter Classification": "American; Irish; Catholic", "Target Classification": "American; Irish; Catholic", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.686965, 42.731883 ] } },
{ "type": "Feature", "properties": { "ID": 910, "year": 1829, "date": "1829\/11", "State": "Pennsylvania", "Sublocation": "Swartara", "Location": "Swartara, Lebanon Co., PA", "Lat": 40.246491, "Long": -76.811295, "Description": "November: Canal men at the great dam rioted. Shots were fired and shilelahs used. The riot is blamed on foreigners.", "Source": "Mniles'Wkly Register, November 28, 1829", "Type": "Unknown", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.811295, 40.246491 ] } },
{ "type": "Feature", "properties": { "ID": 911, "year": 1829, "date": "1829\/11", "State": "Pennsylvania", "Sublocation": "Lancaster", "Location": "Lancaster, Pa", "Lat": 40.031466, "Long": -76.281284, "Description": "November: Two rival groups of democrats, Wolfites and Buchananites each decided to hold a victory declaration at the court house as soon as the court was adjourned. The Wolfites with the Sheriff at the lead, determined to break up the Buchanan celebration. As soon as the court was out they rushed in, had the sheriff elected chairman by a general howl. He was dragged from the bench, the mayor then being nominated. The discord was generating the orators of the day, GB Porter, James Buchanan and Benjamin Chonpaney making a general dirve for the tops of the Judges' desks. There was a general scramble for the high places, windows sills, pillars, tables, benches, and a general fisticuff ensued. The would-be speakers were shouted down and the target for flying inkstands, glasses and pitchers. This lasted for two hours.", "Source": "Klein.Penn. Politics, p.327-328", "Type": "Political", "Rioter Classification": "Political; Democrat; Legal; Police; Sheriff; Elected Official; Mayor; Buchananite; Wolfite", "Target Classification": "Political; Democrat; Legal; Police; Sheriff; Elected Official; Mayor; Buchananite; Wolfite", "Actions": "Non-Lethal Violence; Threat; Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": "GB Porter; James Buchanan; Benjamin Chonpaney", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.281284, 40.031466 ] } },
{ "type": "Feature", "properties": { "ID": 912, "year": 1829, "date": "1829\/11", "State": "Virginia", "Sublocation": "Harrisonburg", "Location": "Hourisonburg, VA", "Lat": 38.444348, "Long": -78.869919, "Description": "November: The effigy of BW Leigh, esq., was burnt in the public square for comments made disparaging west-Virginians and their militia at the state Conventions. The effigy was burnt with a piece of paper in hand, supposedly the comments.", "Source": "Niles'Weekly Register, December 5, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Effigy", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": "BW Leigh", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -78.869919, 38.444348 ] } },
{ "type": "Feature", "properties": { "ID": 913, "year": 1829, "date": "1829\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: The Masonic hall broken up by a number of rioters, attacked some of the persons there and beat Thomas Simmons, keeper of the Hall and the bartender. The watch was called and the group (??) those secured by the watch. This is the 3rd time this kind of disturbance has occurred.", "Source": "SC PC cases 7441 7", "Type": "Anti-Masonic", "Rioter Classification": "Anti-Masonic", "Target Classification": "Masonic", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Thomas Simmons", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 914, "year": 1829, "date": "1829\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Eliah Archer, with others disturbed the methodist meeting house near 3rd avenue during public worship. Archer has done this before. Result: convicted.", "Source": "NYC General Sessions, December 14, 1829", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Eliah Archer", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 915, "year": 1829, "date": "1829\/11", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "November: White and black people fought in Cedar Ward following a loud and emotional religious service.", "Source": "Nash.Forging Freedom, p.275", "Type": "Racial; Religious", "Rioter Classification": "White; Black", "Target Classification": "White; Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 916, "year": 1829, "date": "1829\/11", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "November: \"A riot between blacks and whites arising out of some personal quarrel in which others than the original disputants became involved…; some persons were injured on both sides and some were arrested. This was the beginning of a series of riots in which white people generally the assailants upon the blacks. They were inflamed by prejudice and strong opposition to the doctrines of the friends of the abolition of slavery, which about this time were beginning to be boldly proclaimed.", "Source": "Scharf and Wescott.History of Philadelphia, p.624", "Type": "Racial; Anti-Abolitionist", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 917, "year": 1829, "date": "1829\/12", "State": "Pennsylvania", "Sublocation": "Franklin Co.", "Location": "Franklin Co., Penn", "Lat": 40.018342, "Long": -77.682709, "Description": "December: Journeymen shoemakers tried for conspiring to set prices and wages and compell owners to hire only members of their society. They also corresponded with like societies in Pittsburgh and elsewhere and drove an ex-member out of town. Two men convicted.", "Source": "Niles'Weekly Register, January 2, 1830", "Type": "Labor", "Rioter Classification": "Worker; Shoemaker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.682709, 40.018342 ] } },
{ "type": "Feature", "properties": { "ID": 918, "year": 1829, "date": "1829\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: George Messerve and others harassed and rioted at Letitia Treadwell's in the morning.", "Source": "SC PC cases 7441 10", "Type": "Unknown", "Rioter Classification": "Butcher; Worker", "Target Classification": "Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "George Messerve", "Persons Named as Targets": "Letitia Treadwell", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 919, "year": 1829, "date": "1829\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Bedford St. church was often harassed by groups of young boys who come there, run in and out and laugh, hallow and use indecent language. This occurred on Sunday, the 12th towards the end of the services.", "Source": "SC PC cases 7441 9", "Type": "Religious", "Rioter Classification": "Youth", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 920, "year": 1829, "date": "1829\/12", "State": "Maryland", "Sublocation": "Washington Turnpike", "Location": "Washington Turnpike, Maryland", "Lat": 39.036655, "Long": -76.955409, "Description": "December: Keeper of a turngate was assaulted after he refused passage to two travellers on horseback. The attack was made by the two men and a whole wagon load of men. The lock was broken and the house wrecked and the turnkeeper and his assistant badly beaten. A posse later did not attack the rioters for fear of their numbers. This occurred at 8:00am.", "Source": "NYEP, December 29, 1829", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Turnpike Keeper", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 2, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.955409, 39.036655 ] } },
{ "type": "Feature", "properties": { "ID": 921, "year": 1830, "date": "1830\/01", "State": "Ohio", "Sublocation": "Portsmouth", "Location": "Portsmouth, Ohio", "Lat": 38.750848, "Long": -82.984155, "Description": "January: \"Black Friday\" whites compell eighty black residents to leave town.", "Source": "Werner, p.230", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -82.984155, 38.750848 ] } },
{ "type": "Feature", "properties": { "ID": 922, "year": 1830, "date": "1830\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Theater riot following a masquerade.", "Source": "Weinbaum,M+D, p.37", "Type": "Theater", "Rioter Classification": "Unknown", "Target Classification": "Theater", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 923, "year": 1830, "date": "1830\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Butcher Boys of center market frequently set their dogs on the students of the African free school in Mulberry street. On one occasion they had them attack the instructors son. On another occasion they seized a black girl and held her fast until the dogs had almost torn her clothes off.", "Source": "Common Advertiser, February 27, 1830", "Type": "Racial", "Rioter Classification": "Youth; Butcher; Worker", "Target Classification": "Black; Youth; Woman; School", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 924, "year": 1830, "date": "1830\/03", "State": "Virginia", "Sublocation": "Williamsburg", "Location": "Williamsburg", "Lat": 37.275957, "Long": -76.710047, "Description": "March: Ritos at Wm. And Mary. A student had been suspended. Windows and papers were destroyed.", "Source": "Common Advertiser, March 16, 1830", "Type": "Student", "Rioter Classification": "Student; Youth", "Target Classification": "University", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.710047, 37.275957 ] } },
{ "type": "Feature", "properties": { "ID": 925, "year": 1830, "date": "1830\/03", "State": "New York", "Sublocation": "Rochester", "Location": "Rochester", "Lat": 43.184498, "Long": -77.597144, "Description": "March: Riot a 9 o'clock at Washington House. 15 men came in and demanded drnks without wanting to pay for them. They destroyed the bar and furniture. Some persons who tried to stop the riot were wounded. Four or five of them were arrested.", "Source": "Common Advertiser, March 11, 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.597144, 43.184498 ] } },
{ "type": "Feature", "properties": { "ID": 926, "year": 1830, "date": "1830\/03", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "March: At least 80 sailors in a boarding on Jefferson street met and planned to attack the young man who shot Thomas Martin who was out on bail. A spy for the magistrates was found among them and severely ill-treated. The mayor then arrested the sailors releasing them wth a lecture the next morning.", "Source": "NYEP, March 26, 1830", "Type": "Unknown", "Rioter Classification": "Sailor", "Target Classification": "Spy; Magistrate; Legal", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 927, "year": 1830, "date": "1830\/04", "State": "New Jersey", "Sublocation": "Newark", "Location": "Newark, NJ", "Lat": 40.733983, "Long": -74.172024, "Description": "April: A mob attacked a masonic lodge at night, doing considerable damage.", "Source": "Vaughn.Anti-Masonic Party, p.167", "Type": "Anti-Masonic", "Rioter Classification": "Anti-Masonic", "Target Classification": "Masonic", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.172024, 40.733983 ] } },
{ "type": "Feature", "properties": { "ID": 928, "year": 1830, "date": "1830\/04", "State": "Pennsylvania", "Sublocation": "Easton", "Location": "Easton, PA", "Lat": 40.687964, "Long": -75.211394, "Description": "April: Saturday evening riot at Snufftown, opposite Easton between canal workers and raftsmen. Three raftsmen were injured. Unsure how many canal workers were injured.", "Source": "NYEP, April 16, 1830", "Type": "Unknown", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Worker; Raftsman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.211394, 40.687964 ] } },
{ "type": "Feature", "properties": { "ID": 929, "year": 1830, "date": "1830\/04", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "April: Riot opposite the Franklin Institute. An escaped slave was to be returned to his master and a group of black people attempted to rescue him. They were not successful. A rioter was shot by a deputy.", "Source": "Common Advertiser, April 20, 1830", "Type": "Slavery", "Rioter Classification": "Black; Anti-Slavery", "Target Classification": "Legal; Law Officer", "Actions": "Non-Lethal Violence; Extraction", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 930, "year": 1830, "date": "1830\/05", "State": "Georgia", "Sublocation": "Milledgeville", "Location": "Milledgeville, GA", "Lat": 33.076141, "Long": -83.234242, "Description": "May: Riot among diggers. 50 or 60 Carolinians attempted to drive off 20 or 30 Georgians from the area they were digging. The Carolinians were unsuccessful.", "Source": "Common Advertiser, May 12, 1830", "Type": "Labor", "Rioter Classification": "Digger; Carolinian", "Target Classification": "Digger; Georgian", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -83.234242, 33.076141 ] } },
{ "type": "Feature", "properties": { "ID": 931, "year": 1830, "date": "1830\/06", "State": "Pennsylvania", "Sublocation": "Turkeyfoot", "Location": "Turkeyfoot, PA", "Lat": 39.902413, "Long": -77.749907, "Description": "June: Riot of sorts as 3 brothers and 2 cousins attack commanding Major A. Hanna at a training camp for a volunteer brigade. Near Somerset.", "Source": "Common Advertiser, June 14, 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Military; Officer", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "A. Hanna", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.749907, 39.902413 ] } },
{ "type": "Feature", "properties": { "ID": 932, "year": 1830, "date": "1830\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Meeting houe for religious services disrupted by several young men. Address - 245 Spring.", "Source": "NYC General Sessions, June 11, 1830", "Type": "Religious", "Rioter Classification": "Youth", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 933, "year": 1830, "date": "1830\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: There was fighting corner 86th street and 4th ave. The fighters threatened the spectators and John Daddy's, who lived nearby, house was stoned.", "Source": "NYC General Sessions, June 14, 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Daddy", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 934, "year": 1830, "date": "1830\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Stonecutters objected to the use of granite pillars made in Sing Sing. In the forenoon they assembled in a large body in Broadway opposite construction site of the American Museum on the corner of Ann street. \"They ordered the workers to desist from going on with their work, and used threat to compel them to break off.\" The police office was notified and several of the most active were arrested.", "Source": "NYEP, June 22, 1830", "Type": "Labor", "Rioter Classification": "Worker; Stone Worker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Strike; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 935, "year": 1830, "date": "1830\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Hog cart riot in the afternoon at 1st avenue and North street. \"Several hundred\" included, one hog officer severely injured, the other was dragged through the mud. After this, the mob dispersed. Ringleaders recognized and arrests expected.", "Source": "NYEP, July 2, 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 936, "year": 1830, "date": "1830\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: After a dance at 17th street and 6th ave, early in the morning, a group of men broke into Patrick Lenor and James Scanlow's house, broke windows, etc. and did a lot of damage.", "Source": "NYC SC PC 7442 9, July 6 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Patrick Lenor; James Scanlow", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 937, "year": 1830, "date": "1830\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: About 3 o'clock at least 12 men came to Thomas Edgar's store and \"behaved in a riotous and disorderly manner.\" They assaulted and threatened Edgar.", "Source": "SC PC - cases 7442, 7, August 29, 1830", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Thomas Edgar", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 938, "year": 1830, "date": "1830\/09", "State": "Pennsylvania", "Sublocation": "Dunnstown", "Location": "Dunnstown, Pa", "Lat": 41.14301, "Long": -77.423321, "Description": "September: Canal workers riot in dispute with local Germans over apple picking. One killed.", "Source": "Way.Common Labour, p.288", "Type": "Ethnic\/National", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "German", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.423321, 41.14301 ] } },
{ "type": "Feature", "properties": { "ID": 939, "year": 1830, "date": "1830\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: 12 men arrested for rioting about 2 or 3 am in Water street near Moore street. They attacked a watchman with a club.", "Source": "NYC General Sessions, September 9, 1830", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
