{ "type": "Feature", "properties": { "ID": 89, "year": 1787, "date": "1787\/06", "State": "Massachusetts", "Sublocation": "Lowesboro", "Location": "Lowesboro, Ma", "Lat": 42.517267, "Long": -73.228854, "Description": "June: Party of farmers from NY - Shaysites - raid Rep. John Starkweather's home, manhandling him.", "Source": "Szatmary, p.114", "Type": "Debt; Tax; Law", "Rioter Classification": "Debtor; Shaysite; Farmer; Anti-Tax", "Target Classification": "Elected Official", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": "John Starkweather", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.228854, 42.517267 ] } },
{ "type": "Feature", "properties": { "ID": 90, "year": 1787, "date": "1787\/07", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "July: A woman accused of bring a witch was carried through the streets and hooted and pelted with stones as she passed. Apparently she died a few days later.", "Source": "Cited by Herrill (??) Jonson \"The Sovereign States\"", "Type": "Moral Panic", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Non-Lethal Violence; Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
