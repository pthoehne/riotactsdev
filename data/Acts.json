{ "type": "Feature", "properties": { "ID": 941, "year": 1830, "date": "1830\/09", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "September: Affray between white and black persons on Belkap street. Black people had frequently sassed white people there and this led to the riot. George Stearns suffered a broken leg and one black person was severely wounded. Reportedly \"Walker's Appeal\" had instilled in the African Americans a sense that they had extraordinary privileges.", "Source": "NYEP, September 24, 1830", "Type": "Racial", "Rioter Classification": "White; Black", "Target Classification": "White; Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 2, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "George Stearns", "Persons Named as Targets": "George Stearns", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 942, "year": 1830, "date": "1830\/10", "State": "Pennsylvania", "Sublocation": "Carbondale", "Location": "Carbondale, Pa", "Lat": 41.572129, "Long": -75.501808, "Description": "October: Irish workers fight with native born workers.", "Source": "Grimsted, \"Antebellum Labor\"", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Worker; Irish; Catholic; American", "Target Classification": "Worker; Irish; Catholic; American", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.501808, 41.572129 ] } },
