{ "type": "Feature", "properties": { "ID": 2057, "year": 1862, "date": "1862\/05", "State": "Pennsylvania", "Sublocation": "Cass", "Location": "Cass, PA", "Lat": 40.69242, "Long": -76.26019, "Description": "May: Strikers at the Hecksherville Colliery stopped the pumps that kept the mines free of water. In the Swatara district miners marched to neighboring mines and stopped the engines drawing the coal. The sheriff and a small force failed to disperse the crowd and the governor ordered a battalion to the area. Before it arrived the strike was settled.", "Source": "Palladino.Another Civil War, p.125", "Type": "Labor", "Rioter Classification": "Worker; Striker", "Target Classification": "Employer", "Actions": "Strike; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": "PTH Edits", "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.26019, 40.69242 ] } },
{ "type": "Feature", "properties": { "ID": 2058, "year": 1862, "date": "1862\/05", "State": "Illinois", "Sublocation": "Jacksonville", "Location": "Jacksonville, Illinois", "Lat": 39.7968, "Long": -89.64551, "Description": "May: Constable and six man posse enter Portuguese section - called Madiena - to recover a stolen horse. They were attacked by about 100 armed Portuguese. Later, Fry returned with white mob. But bloodshed avoided. This was referred to as the Battle of Madiera.", "Source": "Doyle.Social Order, p.129", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Portuguese; Catholic", "Target Classification": "Legal; Law Officer", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -89.64551, 39.7968 ] } },
{ "type": "Feature", "properties": { "ID": 2059, "year": 1862, "date": "1862\/07", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "July: 40 slaves attempting to enter the city routed the police and were only subdued by the arrival of Federal troops.", "Source": "Messner. \"Black Violence,\" p.22", "Type": "Slavery; Racial; Law", "Rioter Classification": "Slave; Black", "Target Classification": "Legal; Law Officer; Military; White", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
