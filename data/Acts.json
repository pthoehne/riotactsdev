{ "type": "Feature", "properties": { "ID": 1002, "year": 1831, "date": "1831\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: In Green street, between Brown and Spring, there was a general fight with flats, sticks, shovels, etc. Five persons arrested.", "Source": "SC PC cases 7443 17, December 26, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
