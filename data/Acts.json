{ "type": "Feature", "properties": { "ID": 961, "year": 1831, "date": "1831\/04", "State": "Massachusetts", "Sublocation": "Williamstown", "Location": "Williamston College", "Lat": 42.70558, "Long": -73.211621, "Description": "April: Students disrupted Methodist meeting at Daniel Ebon's house. They made noise inside and when they left they threw a barrel and brass kettle full of water through the window. The faculty were notified and promised to do something.", "Source": "NYEP, May 9, 1831", "Type": "Religious; Student", "Rioter Classification": "Student; Youth", "Target Classification": "Religious; Methodist", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Daniel Ebon", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.211621, 42.70558 ] } },
{ "type": "Feature", "properties": { "ID": 962, "year": 1831, "date": "1831\/04", "State": "Conneticut", "Sublocation": "Hartford", "Location": "Hartford, CT", "Lat": 41.766615, "Long": -72.654915, "Description": "April: race Riot. African Americans attacked a group of white people bothering several black girls. White people driven off, seven or eight injured.", "Source": "Werner, p.273", "Type": "Racial", "Rioter Classification": "Black; White", "Target Classification": "White; Black; Woman; Youth", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -72.654915, 41.766615 ] } },
{ "type": "Feature", "properties": { "ID": 963, "year": 1831, "date": "1831\/04", "State": "Alabama", "Sublocation": "Jackson Co.", "Location": "Jackson Co,. Alabama", "Lat": 34.910627, "Long": -85.852284, "Description": "April: About 20 slicks rode up to William Hall's house, took him to a swamp, held a mock trial for counterfeiting and sentenced him to 50 lashes on his back. Hall claimed personal emnity of his brother-in-law involved.", "Source": "Bragg. \"Capt. Slick,\" p.128-131", "Type": "Vigilante; Law; Gang", "Rioter Classification": "Vigilante; Gang; Slick", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence; Seized Person(s)", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "William Hall", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -85.852284, 34.910627 ] } },
{ "type": "Feature", "properties": { "ID": 964, "year": 1831, "date": "1831\/04", "State": "Conneticut", "Sublocation": "Hartford", "Location": "Hartford, CT", "Lat": 41.766615, "Long": -72.654915, "Description": "April: Mob of white people invade black district inflicting minor damage.", "Source": "Werner, p.230", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -72.654915, 41.766615 ] } },
{ "type": "Feature", "properties": { "ID": 965, "year": 1831, "date": "1831\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: A large crowd gather tumultuously outside John Heart's shop at Chatham street in the late afternoon. Brown challenged him to a fight.", "Source": "NYC General Sessions, May 7, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Brown", "Persons Named as Targets": "John Heart", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 966, "year": 1831, "date": "1831\/05", "State": "Massachusetts", "Sublocation": "Lowell", "Location": "Lowell, MA", "Lat": 42.650502, "Long": -71.318531, "Description": "May: Riot among canal workers.", "Source": "Way.Common Labour, p.288", "Type": "Labor", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.318531, 42.650502 ] } },
{ "type": "Feature", "properties": { "ID": 967, "year": 1831, "date": "1831\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: J. cabinetmakers and\/or coachmakers strike lasts for over a month. Intimidate non-strikers. Beat one man up after he had gone to the police office.", "Source": "Weinbaum,Mobs, p.64", "Type": "Labor", "Rioter Classification": "Worker; Cabinetmaker; Coachmaker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Strike; Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Joseph M. Burger", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 968, "year": 1831, "date": "1831\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: Journeymen cabinetmakers strike lasted over a month. The strikers attmpted to persuade and intimidate journeymen working for less wages for about a month. In the evening of May 3, they beat up Joseph M. Burger after he returned from the police office to notify the police. A dozen journeymen named in court records, four were arrested. The issue was later dropped.", "Source": "Weinbaum, p.64", "Type": "Labor", "Rioter Classification": "Worker; Cabinetmaker; Coachmaker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Strike; Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Joseph M. Burger", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 969, "year": 1831, "date": "1831\/05", "State": "Massachusetts", "Sublocation": "Lowell", "Location": "Lowell, MA", "Lat": 42.650502, "Long": -71.318531, "Description": "May: Riot between Irish and Americans. Irish went from shop to shop collecting wood and metal chips. Occassionally, words were exchanged between the native American laborers and Irish. When an American decided to chastise an Irishman on the 17th and probably beat him, the conflict escalated into a general melee on Tuesday night. On Wednesday, the Americans attacked the Irish huts. Several Irishmen were injured. One infant accidently killed. Magistrates interfered but refused to prosecute because no serious damage done.", "Source": "NYEP, May 24, 1831", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Worker; American; Laborer", "Target Classification": "Worker; Irish; Catholic; Youth", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.318531, 42.650502 ] } },
{ "type": "Feature", "properties": { "ID": 970, "year": 1831, "date": "1831\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: Turnout by journeymen carpenters on Thursday morning they formed a procession and invited other carpenters to join them. They wanted a raise from 11 shillings to 12 shillings a day. No violence.", "Source": "Spectator, May 31, 1831", "Type": "Labor", "Rioter Classification": "Worker; Carpenter", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 971, "year": 1831, "date": "1831\/06", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "June: Strikers riot over cut in wages and probably contractor absconding or taking wages.", "Source": "Grimsted, p.10-11", "Type": "Labor", "Rioter Classification": "Worker", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 972, "year": 1831, "date": "1831\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Printers demanding higher wages.", "Source": "Common Advertiser, June 24, 1831", "Type": "Labor", "Rioter Classification": "Worker", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 973, "year": 1831, "date": "1831\/06", "State": "Conneticut", "Sublocation": "Walingford", "Location": "Walingford, ct", "Lat": 41.454559, "Long": -72.817548, "Description": "June: a group of men enter a man named Wheeler's house with the purpose of \"mobbing\" him. He and his family were in bed, but he pulled a knife and stabbed three of the men. One died instantly, another a little later. The rest ran.", "Source": "Spectator, June 10, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Lethal Violence", "Recorded # Killed": "2", "Recorded # Injured": 1, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Wheeler", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -72.817548, 41.454559 ] } },
{ "type": "Feature", "properties": { "ID": 974, "year": 1831, "date": "1831\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Peter Braisted's place, 118 North, was frequently harassed by Jacob Somerindyke and his gang of followers. They did this on the night of the 7th, when the watch was called the men battled the watch and escaped.", "Source": "SC PC 7443 4m June 8 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Legal; Police; Watchman", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Jacob Somerindyke", "Persons Named as Targets": "Peter Braisted", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 975, "year": 1831, "date": "1831\/06", "State": "Maryland", "Sublocation": null, "Location": "Maryland", "Lat": 39.336873, "Long": -77.073283, "Description": "June: Railroad riot - A contractor cheated the laborers out of their pay. The company wanted to settle by dividing up the remaining money due the contractors. This was less than the money owed to the workers. Some laborers agreed to this, others did not. When the company tried to pay off the laborers objections were made and the agent was prevented from dispersing money. The workers started to tear up the line, 26 mile below Baltimore. No rioters arrested.", "Source": "NYEP, July 5, 1831", "Type": "Labor", "Rioter Classification": "Worker; Railroad Worker", "Target Classification": "Employer; Contractor", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.073283, 39.336873 ] } },
{ "type": "Feature", "properties": { "ID": 976, "year": 1831, "date": "1831\/07", "State": "Alabama", "Sublocation": "Jackson Co.", "Location": "Jackson Co., Alabama", "Lat": 34.910627, "Long": -85.852284, "Description": "July: Slicks seize Alanson Huff, tied him to a tree and flogged him. Huff reported that 500 or 600 men belonged to the Slick company.", "Source": "Bragg. \"Capt. Slick,\" p.128-129", "Type": "Vigilante; Law; Gang", "Rioter Classification": "Vigilante; Gang; Slick", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence; Seized Person(s)", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Large", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": "Alanson Huff", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -85.852284, 34.910627 ] } },
{ "type": "Feature", "properties": { "ID": 977, "year": 1831, "date": "1831\/07", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "July: Battle of the Bogue celebration riot. Protestant Irish Gideon society held a procession in the southern part of the city, going to a large hall in 5th street. Near Walnut. Irish catholics resented shi and attacked the procession. The mayor and the police broke it up. When the Protestants left the hall the violence renewed. Some prominent people, including a (??) magistrate involved. One man stabbed.", "Source": "Spectator, July 19, 1831", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Irish; Protestant; Orangeman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 978, "year": 1831, "date": "1831\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Riot at the wharf where the steamboat President lies. Four men and others were extremely hostile to the crew of the President.", "Source": "SC PC cases 7443 6, July 23, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Sailor; Steamboat", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 979, "year": 1831, "date": "1831\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Samuel Adams' door broken by Verry and others in the morning.", "Source": "NYC General Sessions, August 8 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Verry", "Persons Named as Targets": "Samuel Adams", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 980, "year": 1831, "date": "1831\/08", "State": "Maryland", "Sublocation": "New Market", "Location": "New Market, Maryland", "Lat": 39.382025, "Long": -77.268418, "Description": "August: White (Irish) and black railroad workers began fighting Sunday evening. The disturbance was easily quelled. At 3:00pm rioting broke out again. Local citizens interfered, dispersing the Irish, capturing 20. About 400 Irish regrouped and marched into town to rescue the prisoners. Further violence prevented by intercession of Rev. M. Elroy and CW Weaver, Esq. M Elroy bailed the imprisoned rioters, talked the Irish mob down and led them out of town. Several persons injured.", "Source": "NYEP, August 22, 1831", "Type": "Racial; Labor; Law", "Rioter Classification": "White; Black; Irish; Catholic; Worker; Railroad Worker", "Target Classification": "White; Black; Irish; Catholic; Worker; Railroad Worker", "Actions": "Prison Break; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.268418, 39.382025 ] } },
{ "type": "Feature", "properties": { "ID": 981, "year": 1831, "date": "1831\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Threats (maybe violence) following NY Magdaline Society's charge that the city was the home of 10,000 prostitutes. Aimed against the society.", "Source": "Weinbaum.M+D, p.35", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Reformer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 982, "year": 1831, "date": "1831\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Great Row at 8 Albany street, 5 persons arrested but others were also involved in the fighting and quarrelling. At 4:00pm.", "Source": "SC PC cases 7443 8, August 15, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 983, "year": 1831, "date": "1831\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Great noise and disturbance between 8+9 at night at corner of 8th ave and 20th st. There was fighting, quarrelling, and disorderly conduct by a gang of weavers and others who wanted to \"flog the damn yankee watchmen.\" Four men arrested.", "Source": "SC PC cases 7443 9, August 28, 1831", "Type": "Law", "Rioter Classification": "Worker; Weaver", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 984, "year": 1831, "date": "1831\/08", "State": "Massachusetts", "Sublocation": "Charleston", "Location": "Charleston, MA", "Lat": 42.350273, "Long": -71.060098, "Description": "August: Riot started by \"drunken truckmen and handcartmen from Boston.\" Few details. While attempting to quell the disturbance, Dr. Hard and Mr. Audstin, Paseaster, were injured. Sunday evening.", "Source": "NYEP, September 2, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 2, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Hard; Audstin", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 985, "year": 1831, "date": "1831\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Disturbance in Broadway as three defendants and others were in the street at 1:00am making a great noise \"beating and hallowing in the most vociferous manner.\" In Broadway.", "Source": "SC PC cases 7443 7, August 28, 1831", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 986, "year": 1831, "date": "1831\/09", "State": "Rhode Island", "Sublocation": "Providence", "Location": "Providence, RI", "Lat": 41.8282, "Long": -71.416533, "Description": "September: Olney street riot - Began as a fight in a dance hall in which one black person beat 5 white people. Spilled out into street as black and white sailors fought one another with the African Americans driving the white people out of the neighborhood on the 21st. Next night white crowd returned, destroying two houses. A black person fired on the white people, killing one. The rioting continued. On the 23rd, 25 militiamen were routed by rioters. On the 24th, two militia companies fired on rioters, killing 4.", "Source": "Coftrol.Afro-Yankees, p.56-57", "Type": "Racial", "Rioter Classification": "White; Black; Sailor", "Target Classification": "White; Black; Sailor; Military; Militia", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": "5", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.416533, 41.8282 ] } },
{ "type": "Feature", "properties": { "ID": 987, "year": 1831, "date": "1831\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: At least 2 rioters and others harassed Elizah Smith. They did so in the evening, breaking a lamp and spilling oil on a carpet. One, at least, was a butcher.", "Source": "SC PC cases 7443 16, October 6, 1831", "Type": "Unknown", "Rioter Classification": "Butcher", "Target Classification": "Woman", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Elizah Smith", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 988, "year": 1831, "date": "1831\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Pheoebe Doty, says her house was attacked by a gang of men, led by Butcher Harry Hartwell. In the evening. They do this often.", "Source": "SC PC cases 7443 12, October 7, 1831", "Type": "Unknown", "Rioter Classification": "Butcher", "Target Classification": "Woman", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Harry Hartwell", "Persons Named as Targets": "Phoebe Dotty", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 989, "year": 1831, "date": "1831\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Riot started in afternoon and lasted until evening. A dispute between cartmen moving paving stones blew up into a fight. Spectarors gathered and some joined the fray. About 40 persons involved in the fighting. No police ever broke it up.", "Source": "NYEP, October 4, 1831", "Type": "Unknown", "Rioter Classification": "Worker; Cartman", "Target Classification": "Worker; Cartman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
