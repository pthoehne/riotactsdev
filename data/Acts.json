{ "type": "Feature", "properties": { "ID": 1716, "year": 1855, "date": "1855\/12", "State": "Tennessee", "Sublocation": "Nashville", "Location": "Nashville", "Lat": 36.15802, "Long": -86.77077, "Description": "December: Crowd of rowdies gathered outside Catholic church to disrupt Catholic midnight mass. Remained there for several hours disturbing the peace, but local bishop had cancelled service so greater violence avoided.", "Source": "Gohmann.Pol. Nat. in Tenn., p.126", "Type": "Religious", "Rioter Classification": "Protestant", "Target Classification": "Catholic; Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -86.77077, 36.15802 ] } },
