{ "type": "Feature", "properties": { "ID": 1016, "year": 1832, "date": "1832\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: On the previous Saturday night several men came to Hall's house, broke glasses, were rowdy etc. They left, came back, tore down a fence, broke glass in the windows and did other damage. This about 11 or 12 o'clock.", "Source": "SC PC 7444, July 31, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Hall", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1017, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: John Arthur Stephenson, asst. Health Warden for 8th ward, was obstructed from doing his duty by several people. One put a pitchfork to his chest and threatened to stab him.", "Source": "NYC General Sessions, November 16, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Health Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Arthur Stephenson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1018, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: About 50 persons attacked John Eldred and his workers in 12 ward, compelling them to leave off work on the Public Roads and threatening to break his cub if he did not quit.", "Source": "NYC General Sessions, September 10, 1832", "Type": "Labor", "Rioter Classification": "Unknown", "Target Classification": "Worker", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "John Eldred", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1019, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Hog cart disturbance in the 8th ward. Dennis Dougherty and wife threatened John P. Hall, a Health Officer, becase the latter attempted to collect hog.", "Source": "SC PC 7444, August 15, 1832", "Type": "Unknown", "Rioter Classification": "Hog Cart", "Target Classification": "Health Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Dennis Dougherty", "Persons Named as Targets": "John P. Hall", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1020, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Disorderly house kept by Hannah Corvass. Lots of noise, fighting, etc., for which 12 persons were arrested the night of the 16th.", "Source": "SC PC cases 7444, August 17, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1021, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Hog car riot. Probably 8th ward. 16 hogs released from hog cart and threats uttered.", "Source": "NYC General Sessions, November 16, 1832; SC PC 7444, August 21, 1832", "Type": "Unknown", "Rioter Classification": "Hog Cart", "Target Classification": "Health Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1022, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Benjamin Robinson, constable, attempted to arrest James (??), resisted and collected a large number of persons who behaved riotously and threatened Robinson.", "Source": "NYC General Sessions, November 16, 1832", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Benjamin Robinson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1023, "year": 1832, "date": "1832\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Azel Conklin, Marshall, went to arrest John Mewerring. However, when he tried to do this several people came and rescued him.", "Source": "NYC General Sessions, September 5, 1832", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "John Mewerring", "Persons Named as Targets": "Azel Conklin", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1024, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Fight in Walnut street involving unspecified numbers. When the watch interfered, Michael Lynch claimed that if the watch took one man they would have to take everyone. After Lynch was arrested, the crowd followed the watchmen and stoned them.", "Source": "SC PC 7444, September 2, 1832", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Non-Lethal Violence; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Michael Lynch", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1025, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: 12 men, at least, disturb widow Catherin Haviland. Not much on details except disorderly.", "Source": "SC PC 7444, September 10, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Catherin Haviland", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1026, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Herman Howlett ran a school in the basement of the church in Forsythe and Walker street. On the 11th and other days a gang of boys harrassed him and his school. They broke some windows. They do this sort of thing often.", "Source": "SC PC 7444, September 11, 1832", "Type": "Unknown", "Rioter Classification": "Youth", "Target Classification": "Educator; School; Church", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Herman Howlett", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1027, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Watchman Longhorn had a prisoner, Robert Rerkill, encouraged a mob to attempt a rescue. No location given.", "Source": "SC PC 7444, September 24, 1832", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Robert Rerkill", "Persons Named as Targets": "Longhorn", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1028, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Black Bob's Tavern uptown was disrupted by several men who had pissed on the floor, broke windows, etc.", "Source": "NYC General Sessions, October 12, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Black Bob", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1029, "year": 1832, "date": "1832\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Riot in the evening on the corner of 17th st and 6th ave. No numbers given. Marshall John P. Raymond attempted to arrest one rioter when another pulled out a knife and threatened him.", "Source": "SC PC 7444, September 29, 1832", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "John P. Raymond", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1030, "year": 1832, "date": "1832\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Disturbance outside of the Park theater around Chatham street. People shouting and acting riotously. Four persons arrested for riot as a part of the mob.", "Source": "SC PC 7444, October 7 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1031, "year": 1832, "date": "1832\/10", "State": "Maryland", "Sublocation": "Maryland", "Location": "Maryland", "Lat": 39.336873, "Long": -77.073283, "Description": "October: Oyster Wars on the Eastern shore as local oystermen and officials fight off interlopers from PA and NJ.", "Source": "NYEP, October 26, 1832", "Type": "Economic", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.073283, 39.336873 ] } },
{ "type": "Feature", "properties": { "ID": 1032, "year": 1832, "date": "1832\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: A crowd of men attack Eliza Granger's dwelling, between 8 and 9 o'clock at night. Entering the house they broke windows out.", "Source": "SC PC 7444, October 22, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Eliza Granger", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1033, "year": 1832, "date": "1832\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Election rioting. Brawls at the 7th and 14th ward. On the 1st day of the election a landlord evicted a member of the 5th ward Nat. Rep. Comm. With the help of bullies. During the disturbances at least 50 political toughs involved. The riots occurred at the polls. Several persons prevented from voting.", "Source": "NYEP, November 8, 1832; SC PC 7445 3, November 10, 1832", "Type": "Political", "Rioter Classification": "Political", "Target Classification": "Political", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1034, "year": 1832, "date": "1832\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: A large number of persons rioted at Patrick McCafferty's, corner of Elizabeth and Grand. Not sure if connected to Election. Two persons named. Harrington was involved in election rioting.", "Source": "SC PC 7445 4, November 8, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Harrington", "Persons Named as Targets": "Patrick McCafferty", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1035, "year": 1832, "date": "1832\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: A large body of men came to Peter Brower's home, he was an innkeeper, and acted in a riotous and disorderly manner. 5 persons named in deposition.", "Source": "SC PC 7445 2, November 14, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Peter Brower", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1036, "year": 1832, "date": "1832\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Two black people arrested as escaped slaves. While being taken between City Hall and Bridewell - their case still pending, there was an attempt to rescue them. The two officers escorting them were struck and pushed aside, but several officers came to their assistance and the mob was soon repulsed. Four ringleaders arrested. No white persons involved.", "Source": "NYEP, November 14, 1832; SC PC 7445, November 15, 1832", "Type": "Slavery; Racial", "Rioter Classification": "Black; Anti-Slavery; Abolitionist", "Target Classification": "Legal; Law Officer; Watchman; Slavecatcher", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1037, "year": 1832, "date": "1832\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: In Chatham street, John Edwards set up the Spectaculum, which was some kind of transparency show. At about 8 o'clock in the evening there was a disturbance outside. Over 50 stones were thrown at the building, damaging transparencies, breaking windows, and injuring two persons.", "Source": "SC PC 7445 1, November 29, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Edwards", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1038, "year": 1832, "date": "1832\/12", "State": "New Jersey", "Sublocation": "Camden", "Location": "Camden, NJ", "Lat": 39.944731, "Long": -75.116344, "Description": "December: Nearby an attempt to recapture 3 escaped slaves was opposed by a crowd of both blacks and whites. The slaveowner from Havre de Grace and a friend driven off. At Camden they got reinforcements and returned to arrest several of the assailants. Not sure what happened to the slave.", "Source": "NYEP, December 11, 1832", "Type": "Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.116344, 39.944731 ] } },
{ "type": "Feature", "properties": { "ID": 1039, "year": 1832, "date": "1832\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: At least 15 men came to Mary Brown's and demanded drinks etc., between 10 and 11 o'clock at night. They were armed with bludgeons and came with a pot of tar to threaten Brown with tar and feathering. The watch arrived and prevented the destruction of too much furniture.", "Source": "SC PC 7445, December 18, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant; Woman", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Mary Brown", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1040, "year": 1832, "date": "1832\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Broad stret riot. No real details.", "Source": "NYEP, December 31, 1832", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1041, "year": 1832, "date": "1832\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Approximately 40 to 50 persons rioted between 3 and 4 o'clock in the afternoon on Christmas day. Most seem to have been Irish. Several stones were thrown and some fighting. There may have been an attack against a black person. Occurred on 3rd avenue.", "Source": "SC PC 7445 6, December 26, 1832", "Type": "Ethnic\/National; Religious; Racial", "Rioter Classification": "Irish; Catholic", "Target Classification": "Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1042, "year": 1833, "date": "1833\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Riot in Harlem. Two men arreste for a Sunday afternoon frolic in Yorkville and Harlem. Several houses were attacked and a number of gentlemen were beaten. Along 2nd ave.", "Source": "NYEP, August 1, 1833", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1043, "year": 1833, "date": "1833\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Laborers and gentlemen mix it up in the evening at 3rd ave and 19th. Two carriages going downtown came a bit too close to a line of cart taking dirt to be dumped. The Irish laborers driving the cart and others drinking in a nearby grocer objected. A scuffle ensued which had to be broken up by the police.", "Source": "NYC General Sessions, CC - 6", "Type": "Ethnic\/National; Religious; Class", "Rioter Classification": "Irish; Catholic; Worker; Laborer; Gentleman", "Target Classification": "Irish; Catholic; Worker; Laborer; Gentleman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1044, "year": 1833, "date": "1833\/08", "State": "Pennsylvania", "Sublocation": "Pennsylvania", "Location": "Pennsylvania", "Lat": 40.932085, "Long": -77.601813, "Description": "August: German canal worker, Charles Hickenbottom, murdered by about 20 other canal workers. One worker had been dismissed, 20 of his fellow workers quit in sympathy. They then went down to the canal about one mile and attacked Hickenbottom. 11 arrested and charged with murder. Five charged with A+B and riot.", "Source": "NYEP, August 6, 1833", "Type": "Labor; Ethnic\/National", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Worker; Canal Worker; German", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.601813, 40.932085 ] } },
{ "type": "Feature", "properties": { "ID": 1045, "year": 1833, "date": "1833\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Peter Kusick convicted of an assault on a watchman in a riot in East Broadway. The police had to quell it. Kusick used an ax. Tried for assault.", "Source": "The Court and NY Enquirer, Dec 10, 1833", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": "Peter Kusick", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1046, "year": 1833, "date": "1833\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Church on 2nd street harassed. Young men shouting \"hot corn\" and acting disorderly. A watchman arrested three persons, but the rest continued the disturbance.", "Source": "NYCSCPCC 7445 13", "Type": "Unknown", "Rioter Classification": "Youth", "Target Classification": "Unknown", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1047, "year": 1833, "date": "1833\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: Hog cart riot - supposedly a \"gang of half naked, whooping and barbarous negroes.\" In evening the hog collectors were surrounded by an indignant mob, pelted with stones, and escaped only with great difficulty.", "Source": "NYEP, August 23, 1833", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1048, "year": 1833, "date": "1833\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: The collectors of fines for a militia regiment was assaulted with rotten eggs while collecting fines. He had to save himself by jumping several fences.", "Source": "NYEP, August 23, 1833", "Type": "Tax", "Rioter Classification": "Anti-Tax", "Target Classification": "Tax Collector; Militia", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1049, "year": 1833, "date": "1833\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: Two or three rows occurred when the books for subscription were opened for certain banks", "Source": "NYEP, August 23, 1833", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1050, "year": 1833, "date": "1833\/08", "State": "Pennsylvania", "Sublocation": "Dunnsburg", "Location": "Dunnsburg, PA", "Lat": 41.247361, "Long": -76.979092, "Description": "August: Riot between laborers working on the Great Island Dam and a number of boatmen. On Saturday morning a dispute over a boatload of stone, the laborers wanted it placed on the dam, led to a riot which continued until Sunday evening. One Irish laborer was killed. Several people were wounded. The militia was called out Sunday to quell the disturbance.", "Source": "Common Advertiser, August 30, 1833", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Worker; Irish; Catholic; Boatman", "Target Classification": "Worker; Irish; Catholic; Boatman", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.979092, 41.247361 ] } },
{ "type": "Feature", "properties": { "ID": 1051, "year": 1833, "date": "1833\/09", "State": "New York", "Sublocation": "Manchester", "Location": "Manchester, NY", "Lat": 42.970008, "Long": -77.228299, "Description": "September: A peace officer was killed while attempting to quell a disturbance among some drunken rioters.", "Source": "Com. Adv., Sep. 17, 1833", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.228299, 42.970008 ] } },
{ "type": "Feature", "properties": { "ID": 1052, "year": 1833, "date": "1833\/09", "State": "New Jersey", "Sublocation": "New Brunswick", "Location": "New Brunswick, NJ", "Lat": 40.495172, "Long": -74.4311, "Description": "September: Riot at the race course between workers on the Delaware and Rariton cancal and local citizens. 8 or 10 rioters injured. One, a physician from New Brunswick, was so seriously injured his life was in danger.", "Source": "NYEP, Sep. 23, 1833", "Type": "Unknown", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Worker; Canal Worker", "Actions": "Non-Lethal Violence; Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.4311, 40.495172 ] } },
{ "type": "Feature", "properties": { "ID": 1053, "year": 1833, "date": "1833\/09", "State": "Conneticut", "Sublocation": null, "Location": "CT", "Lat": 41.538474, "Long": -72.622898, "Description": "September: Assault made on Miss Crandall's house while a clergyman held a meeting there. Rotten eggs and other missiles thrown at windows. Offal dumped into well on another occasion.", "Source": "31 Cong., 1 sess., App. 1059", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Woman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Crandall", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -72.622898, 41.538474 ] } },
{ "type": "Feature", "properties": { "ID": 1054, "year": 1833, "date": "1833\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Anti-abolitionists gather outisde Clinton Hall where an abolitionist meeting had been scheduled. Garrison was supposed to attend. The meeting had been shifted to the Chatton street chapel. The antis went to Tammary hall and then marched to the chapel, arriving after the abolitionists left. The antis broke in, dressed a black \"Arthur Tappon\" and mockingly passed resolutions for immediate emancipation.", "Source": "Richards, p.27-30", "Type": "Slavery; Anti-Abolitionist", "Rioter Classification": "Anti-Abolitionist; Pro-Slavery", "Target Classification": "Abolitionist; Anti-Slavery; Church", "Actions": "Effigy; Humiliation", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Arthur Tappan", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1055, "year": 1833, "date": "1833\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Disturbance at Jewish synagogue. Internal problem over the correct rituals. Ordinarily two leaders from the congregation read passages from the Penta (??). But they usually were not from the same family. When a father attempted to read after his son, the congregation objected and had to physically throw him out.", "Source": "Com. Adv., October 9, 1833", "Type": "Religious", "Rioter Classification": "Religious; Jewish", "Target Classification": "Religious; Jewish", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1056, "year": 1833, "date": "1833\/10", "State": "Missouri", "Sublocation": "Jackson Co.", "Location": "Jackson Co., Missouri", "Lat": 39.074342, "Long": -94.468339, "Description": "Oct-Nov: Missourian expell Mormons, burning houses, demolishing furniture, cornfields, horsewhipping men.", "Source": "Baily, p.8", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Mormon", "Actions": "Property Destroyed; Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -94.468339, 39.074342 ] } },
{ "type": "Feature", "properties": { "ID": 1057, "year": 1833, "date": "1833\/12", "State": "South Carolina", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "December: A group of drunken Irish beat a native American (WASP?) to death. Next night 500 activists marched on Irish. Troops called, but stood helplessly as several houses were destroyed.", "Source": "Bisson,Countdown to Violence, p.87-89", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Irish; Catholic", "Actions": "Property Destroyed; Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": "PTH edits", "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 1058, "year": 1833, "date": "1833\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Tumultuous meeting at Tammary Hall between Bowne supporters and G. Wells supporters. Both supported Jackson. Unsure of how much violence there was.", "Source": "Howe,DiaryVII, November 1, 1833", "Type": "Political", "Rioter Classification": "Political", "Target Classification": "Political", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1059, "year": 1833, "date": "1833\/11", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "November: Riot on Chestnut street wharf. Some of the passengers in the Balta Steamboat, laborers, started fighting. The stores they threw hit others and the peace officers were forced to interfere, beating the rioters.", "Source": "NYEP, November 5, 1833", "Type": "Unknown", "Rioter Classification": "Worker; Laborer", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1060, "year": 1833, "date": "1833\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Some violence at meetings of journeymen tailors on strike to maintain meetings. With over 2,000 tailors there were difficulties in maintaing solidarity. Some workers returned to shops for below standard wages.", "Source": "Morning CourierNovember 16, 1833", "Type": "Labor", "Rioter Classification": "Worker; Tailor", "Target Classification": "Worker; Tailor; Strikebreaker", "Actions": "Non-Lethal Violence; Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1061, "year": 1833, "date": "1833\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Piano Forte workers on strike harassed Benjamin Eastmond because of his refusal to join the strike. They followed him about the street, even into the shop, threatening him. On the evening of the 20th they assaulted him, tearing his court. Twelve men are named. Maybe more involved.", "Source": "NYC SC PC - 7446", "Type": "Labor", "Rioter Classification": "Worker", "Target Classification": "Worker; Strikebreaker", "Actions": "Strike; Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Benjamin Eastmond", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1062, "year": 1833, "date": "1833\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Piano makers on strike for wages. Hassle non-strikers, threatening them and on the 20th assaulting them. Lasted several days at least.", "Source": "NYC SC PC - 7446", "Type": "Labor", "Rioter Classification": "Worker", "Target Classification": "Employer; Worker; Strikebreaker", "Actions": "Strike; Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1063, "year": 1833, "date": "1833\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Threater riot at Richmond Hill theater interrupted performance. Police were attacked as was the door keeper. 11 persons named.", "Source": "NYC SC PC - 7446 3. Howes v Raulet et al, Nov 26,1833", "Type": "Theater", "Rioter Classification": "Unknown", "Target Classification": "Theater; Legal; Law Officer", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1064, "year": 1833, "date": "1833\/11", "State": "Maryland", "Sublocation": "Chesapeake Bay", "Location": "Chesapeake Bay", "Lat": 39.460764, "Long": -76.095696, "Description": "November: Maryland\/Penn oyster war.", "Source": "NY Eng, November 25, 1833", "Type": "Economic", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.095696, 39.460764 ] } },
{ "type": "Feature", "properties": { "ID": 1065, "year": 1833, "date": "1833\/11", "State": "South Carolina", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "November: Irish and protestant Americans fight during Thanksgiving celebration at Roger McGowan's restaurant. One yankee died in the fight.", "Source": "Hammet, \"Two mobs…,\" p.899", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; American; Protestant", "Target Classification": "Irish; Catholic; American; Protestant", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 1066, "year": 1833, "date": "1833\/11", "State": "South Carolina", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "November: When Benjamin Daniels looked into an Irish dancing party through a window, his gaping was greeted by an attack. So terrific was the beating that Daniels died the next day. Engraged by Daniels' death, a mob formed the next evening and attempted to demolish Roger McGowan's house, the site of the Irish dance. The navy sent some men who temporarily halted the destruction, but as soon as they left, the mob finished its work.", "Source": "Com. Adv., December 2, 1833", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; American; Protestant", "Target Classification": "Irish; Catholic; American; Protestant", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Benjamin Daniels", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 1067, "year": 1833, "date": "1833\/12", "State": "New York", "Sublocation": "Utica", "Location": "Utica, NY", "Lat": 43.103722, "Long": -75.241196, "Description": "December: After a week of popular agitation about colonization, a mob burned Beriah Green, Pres. Of Oneida Institute and against colonization and Alvin Stewart, another anti-slavery man, in effigy.", "Source": "Richards, p.86", "Type": "Slavery", "Rioter Classification": "Colonization; Anti-Abolitionist", "Target Classification": "Abolitionist; Anti-Slavery; Anti-Coloniztion", "Actions": "Effigy", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Beriah Green; Alvin Stewart", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.241196, 43.103722 ] } },
{ "type": "Feature", "properties": { "ID": 1068, "year": 1833, "date": "1833\/12", "State": "Missouri", "Sublocation": null, "Location": "Missouri", "Lat": 39.135667, "Long": -92.776356, "Description": "December: Notice of Mormon war.", "Source": "Com. Adv., December 17, 1833", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Mormon", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -92.776356, 39.135667 ] } },
{ "type": "Feature", "properties": { "ID": 1069, "year": 1833, "date": "1833\/12", "State": "Maryland", "Sublocation": "Lowell", "Location": "Lowell, Md", "Lat": 42.650502, "Long": -71.318531, "Description": "December: From Boston Journal comes a report of an anti-Avery mob in which an effigy of Avery shot and burnt because he preached at the Methodist church on Chapel Hill.", "Source": "Court NYC, Dec 27, 1833", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Effigy", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.318531, 42.650502 ] } },
{ "type": "Feature", "properties": { "ID": 1070, "year": 1833, "date": "1833\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: For over two hours the Bowers Theater was disturbed by several persons. The theater manager had recently dismissed Samuel \"Treat\" McKinney. Apparently his supporters were going to create a ruckus until he was reinstated.", "Source": "NYC General Sessions People vs. McKinney et al., January 16, 1834", "Type": "Theater", "Rioter Classification": "Unknown", "Target Classification": "Theater", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1071, "year": 1833, "date": "1833\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: About 20 men acted riotously and turbulently at Phoebe Duty's house, destroying furniture, etc.", "Source": "NYC General Sessions, People vs O'Mera, Jan 15, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Phoebe Dotty", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1072, "year": 1833, "date": "1833\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: There was a disturbance during services at the Reformed Presbyterian Church in Chambers street, under Rev. McLeod. Eight persons are named by others were involved. Because of the clamor and noise the services were stopped.", "Source": "SC PC cases 7445, 9, January 14, 1833", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Church; Presbyterian", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "McLeod", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1073, "year": 1833, "date": "1833\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: 20 to 40 ruffians entered at least 4 inns along a sleighing road and disrupted them. They beat proprietors and injured at least 6 persons.", "Source": "Weinbaum.M+D, p.65-66", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 6, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1074, "year": 1833, "date": "1833\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Riot at the Park Theater in the evening. Rioters accompanied by Prostitutes very disorderly and theeatened Marshall.", "Source": "NYC General Sessions, March 13, 1833", "Type": "Law; Theater", "Rioter Classification": "Woman; Prostitute", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1075, "year": 1833, "date": "1833\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Theater riot for un-ascertainable reasons.", "Source": "Weinbaum.M+D, p.37", "Type": "Theater", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1076, "year": 1833, "date": "1833\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Election riot at the 6th ward poll. David Gilmartin identified as a ringleader who assaulted several people.", "Source": "SC PC 7445 11, April 10, 1833", "Type": "Political", "Rioter Classification": "Political", "Target Classification": "Political", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "David Gilmartin", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1077, "year": 1833, "date": "1833\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: Strike by journeymen carpenters for higher wages. Parades through the streets to get support for the strike from some strikers. Wanted a raise from $1.32 to $1.50.", "Source": "P. Howe Diary VI, May 17, 1833", "Type": "Labor", "Rioter Classification": "Worker; Carpenter", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1078, "year": 1833, "date": "1833\/06", "State": "Michigan", "Sublocation": "Detroit", "Location": "Detroit", "Lat": 42.405935, "Long": -83.104235, "Description": "June: A mob of about 40 black people rescued an escaped slave from his owner. They surronded and beat the sherriff. A posse chased the black people into the countryside and managed to arrest a few but the (??) escaped. A committee was formed to investigate the riot.", "Source": "Schneider. \"Urbanization…Detroit 1824-1847,\"Michigan History, p.263-265", "Type": "Slavery; Racial; Law", "Rioter Classification": "Black; Anti-Slavery", "Target Classification": "Slaveholder; Legal; Law Officer; Sheriff", "Actions": "Non-Lethal Violence; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -83.104235, 42.405935 ] } },
{ "type": "Feature", "properties": { "ID": 1079, "year": 1833, "date": "1833\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Riot in the 1st ward. Watchman hit by cart ring while rioters attampt rescue. Rioters from 6th ward.", "Source": "NYC General Sessions, July 8, 1833", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1080, "year": 1833, "date": "1833\/06", "State": "New York", "Sublocation": "New York City", "Location": "Brooklyn", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Turnout of ropemakers objecting to the introduction of labor saving machinery. On the 17th abuot 200 ropemakers from NYC and Bklyn marched to Lewis' ropewalk at the Walkabout to seize rope manufactured there with the new marhcinery. They talked it over with the owner and bought it from him. They then paraded through Bklyn, with drum and fife and burnt it in a bon fire at Fort Green.", "Source": "NYEP, June 19, 1833", "Type": "Labor", "Rioter Classification": "Worker; Ropemaker", "Target Classification": "Employer", "Actions": "Strike; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Lewis", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1081, "year": 1833, "date": "1833\/07", "State": "Missouri", "Sublocation": "Independence", "Location": "Independence, Missouri", "Lat": 39.084331, "Long": -94.427416, "Description": "July: Missourians drive Mormons out. Destroyed press and office of Mormon Star, attacked mercantile property owned by Mormons and tarred and feathered two church elders who refused to renounce the book of Mormon.", "Source": "Baily, p.6-7", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Mormon; Merchant; Press", "Actions": "Property Destroyed; Humiliation", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -94.427416, 39.084331 ] } },
{ "type": "Feature", "properties": { "ID": 1082, "year": 1833, "date": "1833\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Several men attacked John Dougherty, master of steamboat Fairhour, aboard the boat.", "Source": "SC PC 7445 12, July 25, 1833", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Steamboat Master", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Dougherty", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1083, "year": 1833, "date": "1833\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Watchman Henry Hinson responded to a cry of distress from a house, corner of Pelham and Cherry streets. He and two other watchmen were assaulted by at least 11 men and coomen. This between 11 and 12 o'clock at night.", "Source": "NYC General Sessions, February 8, 1833", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Henry Hinson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1084, "year": 1833, "date": "1833\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: On Sunday morning a number of men rushed into Silvester D. Judson's porter room and acted \"in a riotous fashion.\" Judson had to sseek safety by going to the police. Three persons named.", "Source": "SC PC 7445 12, July 29, 1833", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Merchant", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Silvester D. Judson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1085, "year": 1834, "date": "1834\/01", "State": "Pennsylvania", "Sublocation": "Lancaster", "Location": "Lancaster, Penn", "Lat": 40.031466, "Long": -76.281284, "Description": "January: Workers use col (??) violence during strike concerning wages. Pennsylvania canal workers involved.", "Source": "Geinstel, \"A-C,\" 11,23,40", "Type": "Labor", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Employer", "Actions": "Strike; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.281284, 40.031466 ] } },
{ "type": "Feature", "properties": { "ID": 1086, "year": 1834, "date": "1834\/01", "State": "Maryland", "Sublocation": "Williamsport", "Location": "Williamsport, Md", "Lat": 39.595218, "Long": -77.821478, "Description": "January-Feb: Canal workers from Cork fight with workers from Longford in an effort to protect their jobs. Several battles fought, resulting in 6 deaths.", "Source": "Grinsted. \"Ante-Bellum Labor,\" JSH (1965), p.9", "Type": "Labor; Gang", "Rioter Classification": "Worker; Canal Worker; Irish; Catholic; Gang", "Target Classification": "Worker; Canal Worker; Irish; Catholic; Gang", "Actions": "Lethal Violence", "Recorded # Killed": "6", "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.821478, 39.595218 ] } },
{ "type": "Feature", "properties": { "ID": 1087, "year": 1834, "date": "1834\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: Three Kelsos with several others broke into Mary James' 448 Water street house, Screwdock hotel, set fire to her greens, broke and destroyed several articles of clothing and acted riotously. In the evening.", "Source": "NYC SC PC - cases 7446 9, James v Kelso et al., Jan 24, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Mary James", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1088, "year": 1834, "date": "1834\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: \"A number of boys and men\" disturbed and disquited the congregation of the Chatham street chapel on their way to and from services. Such crowds had collected there recently on several occasions.", "Source": "NYC SC PC - cases 7446 6, Smith v. Brooks et al, Feb. 3, 1834", "Type": "Religious", "Rioter Classification": "Youth; Men", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1089, "year": 1834, "date": "1834\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Public meeting at the Park, called by the Whigs, somewhat disorderly, as pro-Bank whigs and anti-Bank Jacksonians struggled to gain control over the platform and chairmanship. Some pushing and shoving. Eventually anti-bank men controlled the platform, tore it down, and paraded through the streets with it. Standard political meeting disorder. NYEP symptathetic to anti-bank.", "Source": "NYEP, Feb. 8 and 10, 1834", "Type": "Political; Economic", "Rioter Classification": "Political; Democrat; Anti-Bank", "Target Classification": "Political; Whig; Pro-Bank", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1090, "year": 1834, "date": "1834\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Riotous attack on Michael Farrel's sailor's boarding house at 350 Water street. Furniture was broken, propery injured, he was robbed of $150 and threatened with further action. There had earlier been a meeting at Harmony Hall to discuss the sailor's interests. Those in the mob are identified by testimony as sailors or other boarding housekeepers. Might be some kind of worker's action.", "Source": "NYC SC PC 7446 6, Farrel v. Roe et al., F. 15, 1834", "Type": "Labor", "Rioter Classification": "Sailor; Merchant", "Target Classification": "Merchant; Boardinghouse", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Michael Farrel", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1091, "year": 1834, "date": "1834\/03", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "March: A group of Catholics attacked Samuel B. Smith as he addressed a Baptist group. He escaped bodily injury.", "Source": "Billington, p.60", "Type": "Religious", "Rioter Classification": "Religious; Catholic", "Target Classification": "Religious; Baptist", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Samuel B. Smith", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 1092, "year": 1834, "date": "1834\/03", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "March: Faction fight among canal workers. Several killed. (??) guards ordered out.", "Source": "Way.Common Labour, p.195,289", "Type": "Labor", "Rioter Classification": "Worker; Canal Worker", "Target Classification": "Worker; Canal Worker", "Actions": "Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 1093, "year": 1834, "date": "1834\/03", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "March: Pro Jackson, anti-bank meeting of Democrats at Faneuil Hall attacked by pro-bank whig mob. Democrats held their ground and drove Whigs out.", "Source": "Bisson.Countdown to Violence, p.93", "Type": "Political; Economic", "Rioter Classification": "Political; Whig; Pro-Bank", "Target Classification": "Political; Democrat; Anti-Bank", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 1094, "year": 1834, "date": "1834\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Election riots on two days between the anti-Bank Jacksonians and the Whigs. There was a strong ehtnic content to the disturbance as the 1st days dioting started when Whigs visiting the 6th ward polls harassed the Irish. The second day saw fighting in the 6th ward, by the Masonic hall and elsewhere. The Whig even attempted to get weapons from the armory but were persuaded not to by officials. The militia was called out.", "Source": "Weinbaum.Mobs and Demagogues, p.3-10", "Type": "Political; Economic", "Rioter Classification": "Political; Whig; Pro-Bank; Nativist", "Target Classification": "Political; Democrat; Anti-Bank; Irish; Catholic", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1095, "year": 1834, "date": "1834\/04", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "April: A crowd guts the Lalaurie Mansion. Earlier, when there was a fire at the place, it was discovered that Madame Lalaurie had tortured her slaves and then chained several in the attic. That night, when it appeared officials were not going to do anything about it, the crowd outside the mansion becamse threatening. The Lalauries escaped and the house was gutted thereafter. Supposedly, the black butler who drove the carriage holding the escaping was caught by a mob and killed.", "Source": "Ashara.French Quarter, p.247-252", "Type": "Slavery; Vigilante; Law", "Rioter Classification": "Vigilante; Anti-Slavery", "Target Classification": "Slaveholder; White; Black; French", "Actions": "Lethal Violence; Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Lalaurie", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 1096, "year": 1834, "date": "1834\/04", "State": "New York", "Sublocation": "Buffalo", "Location": "Buffalo", "Lat": 42.880973, "Long": -78.845114, "Description": "April: News of NY election sent pro-Bank forces into the streets celebrating victory. A lot of singing, carousing, and a few fights. In the streets until 3:00pm.", "Source": "no source given", "Type": "Political; Economic", "Rioter Classification": "Political; Whig; Pro-Bank", "Target Classification": "Political; Democrat; Anti-Bank", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -78.845114, 42.880973 ] } },
{ "type": "Feature", "properties": { "ID": 1097, "year": 1834, "date": "1834\/04", "State": "New Hampshire", "Sublocation": "Porstsmouth", "Location": "Portsmouth, NH", "Lat": 43.077091, "Long": -70.751494, "Description": "April: Election riot. Whigs celebrated success in NY during the day. That evening Democrats set fire to tar barrels and rolled them in streets; then they burnt Henry Clay and Daniel Webster in effigy. They stole and smashed a fire engine to prevent its being used. Then they went to the branch office of the Bus and ripped down the eagle and sign and thew them into the bonfire. Police efforts to interrupt failed. Earlier news article.", "Source": "Common Advertiser, April 29, 1834", "Type": "Political; Economic", "Rioter Classification": "Political; Democrat; Anti-Bank", "Target Classification": "Political; Whig; Pro-Bank", "Actions": "Effigy; Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Henry Clay; Daniel Webster", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -70.751494, 43.077091 ] } },
{ "type": "Feature", "properties": { "ID": 1098, "year": 1834, "date": "1834\/04", "State": "Massachusetts", "Sublocation": "Mansfield", "Location": "Mansfield, MA", "Lat": 42.023918, "Long": -71.216477, "Description": "April: 400 R.R. laborers struck for higher wages and armed with clubs, assaulted one of the contractors. The High Sheriff of Norfolk called out the militia, and arrested nine ringleaders. The deputy sheriff also caleld on the militia and arrested six.", "Source": "Common Advertiser, April 25, 1834", "Type": "Labor", "Rioter Classification": "Worker; Railroad Worker", "Target Classification": "Employer; Railroad", "Actions": "Strike; Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.216477, 42.023918 ] } },
{ "type": "Feature", "properties": { "ID": 1099, "year": 1834, "date": "1834\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Sailor's riot at Munson's boarding house, Number 62 Cherry street. Conflicting stories. One says Munson shipped sailors out with less than the standard advance. As a result, 100 sailors attacked his house and abused his barkeeper. Another report says that Munson was head of a sailor's organization. After a meeting some sailors were misled by a Munson rival and then engaged in the destruction.", "Source": "M. Court. NY Eng, April 25, 1834", "Type": "Labor", "Rioter Classification": "Sailor", "Target Classification": "Merchant", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Munson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1100, "year": 1834, "date": "1834\/04", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "April: On the 23rd large anti-Jackson rally at Monument square. Some violence with pro-Jackson people. Following evenign young whigs \"attacked on South street, by democrats, but Mayor and posse interfered and restored peace.\" NYEP calls this a small disturbance over pro-Bank meeting. Some windows broken.", "Source": "Scharf.Chronicles, p.468-469;NYEP, April 28, 1834", "Type": "Political; Economic", "Rioter Classification": "Political; Whig; Pro-Bank; Youth", "Target Classification": "Political; Democrat; Anti-Bank", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 1101, "year": 1834, "date": "1834\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Engine 24 attacked in the afternoon while putting out a fire in Oliver street. Later, 18 men were arrested at an unlicensed tippling house run by a French man named Bentig. Others arrested Monday morning. Original fight at fire was between rival fire companies. Fighting also occurred at engine house in Wooster street.", "Source": "Common Advertiser, April 29, 1834", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Firefighter", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1102, "year": 1834, "date": "1834\/04", "State": "Massachusetts", "Sublocation": "Mansfield", "Location": "Mansfield, MA", "Lat": 42.023918, "Long": -71.216477, "Description": "April: Irish workers demand higher wages and more grog. They beat contractor when he refused to comply immediately.", "Source": "Grimsted, \"Ante-bellum Labor,\" p.10,11", "Type": "Labor", "Rioter Classification": "Worker; Irish; Catholic", "Target Classification": "Employer; Contractor", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.216477, 42.023918 ] } },
{ "type": "Feature", "properties": { "ID": 1103, "year": 1834, "date": "1834\/04", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "April: Madame LaFaurie's house destroyed by a mob. Reason unsure. Other houses also threatened. Two black persons saved from the fire at Lafaurie's house later died. In the yard and in the old well bodies found. The civil military authorities prevented any further damage.", "Source": "Common Advertiser, May 2, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Woman", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "LaFaurie", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 1104, "year": 1834, "date": "1834\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: Anti-abolitionist disturbance.", "Source": "Richards, p.113", "Type": "Slavery; Anti-Abolitionist", "Rioter Classification": "Anti-Abolitionist", "Target Classification": "Abolitionist", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1105, "year": 1834, "date": "1834\/05", "State": "Massachusetts", "Sublocation": "Cambridge", "Location": "Cambridge, MA", "Lat": 42.372862, "Long": -71.122044, "Description": "May: Disorder at Harvard began as a confrontation between a student and an instructor. Students wrecked the instructor's room and furniture when he reported the student. Disturbances continued with more attacks on property and even battles with the watch. Ultimately entire sophomore class dismissed.", "Source": "Bisson.Countdown to Violence, p.95-99", "Type": "Student", "Rioter Classification": "Student; Youth", "Target Classification": "University; Legal; Law Officer", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.122044, 42.372862 ] } },
{ "type": "Feature", "properties": { "ID": 1106, "year": 1834, "date": "1834\/05", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "May: Black people riot to protect an ex-slave. A Virginia gentleman claimed a black man as a runaway. Black people attempted to rescue him. But they were unsuccessful. Some were arrested. Six were convicted and sent to jail for 8 or 9 months.", "Source": "Mor. Court and NY Eng, May 5, 1834", "Type": "Racial; Slavery", "Rioter Classification": "Black; Anti-Slavery", "Target Classification": "Slaveholder; White", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1107, "year": 1834, "date": "1834\/06", "State": "Missouri", "Sublocation": "Jackson Co.", "Location": "Jackson Co., Missouri", "Lat": 39.074342, "Long": -94.468339, "Description": "June: News of the approach of Joseph Smith and armed Mormons triggered a violence Missouri response. The Missourans stormed the jail, took every fighting weapon Mormons had, and burnt down all remaining Mormon property.", "Source": "Baily.Armres, p.19-20", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Mormon", "Actions": "Property Destroyed; Seized Property", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -94.468339, 39.074342 ] } },
{ "type": "Feature", "properties": { "ID": 1108, "year": 1834, "date": "1834\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Three individual cases of riot reported in the Court of S. S. listing of offenses.", "Source": "NYEP, July 9, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1109, "year": 1834, "date": "1834\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: J. Bakers strikes for higher wages. Strike sanctioned by the Trades Union. They want $1.00 per barrel for baking and a 9 barrel week…some bakers willing to comply. Fear of outside workers expressed.", "Source": "MC and NYE, June 9 and 19, 1834;NYEP, June 9, 1834", "Type": "Labor", "Rioter Classification": "Worker; Baker", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1110, "year": 1834, "date": "1834\/06", "State": "New York", "Sublocation": "Albany", "Location": "Albany", "Lat": 42.65391, "Long": -73.76428, "Description": "June: Theater riot. A visint Irish actor, Power, refused to perform one night because the audience was not big enough. The nex ttheater was crowded and booed and hissed his performance. Not too much was thrown.", "Source": "M. Court. And NY Eng, June 14, 1834", "Type": "Theater", "Rioter Classification": "Unknown", "Target Classification": "Theater; Actor", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Power", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.76428, 42.65391 ] } },
{ "type": "Feature", "properties": { "ID": 1111, "year": 1834, "date": "1834\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Turnout among J. cordwainers of ladies shoes. Not industry-wide; confined to those employees who were trying to lower wages.", "Source": "NYEP, June 14, 1834", "Type": "Labor", "Rioter Classification": "Worker; Cordwainer", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1112, "year": 1834, "date": "1834\/06", "State": "Maryland", "Sublocation": "Maryland", "Location": "Maryland", "Lat": 39.109037, "Long": -76.88685, "Description": "June: Laborers on Washington railroad rioted. Fight was between Irish factions and four persons were killed. 30 or 40 rioters arrested. Militia from Baltimore ordered out to quell riot. Factions: Fardowns and Corkeans - one of those killed was a woman.", "Source": "Common Advertiser, June 19, 20, 1834", "Type": "Labor; Gang", "Rioter Classification": "Worker; Irish; Catholic; Railroad Worker; Gang", "Target Classification": "Worker; Irish; Catholic; Railroad Worker; Woman; Gang", "Actions": "Lethal Violence", "Recorded # Killed": "4", "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.88685, 39.109037 ] } },
{ "type": "Feature", "properties": { "ID": 1113, "year": 1834, "date": "1834\/06", "State": "Maryland", "Sublocation": "Maryland", "Location": "Maryland", "Lat": 39.109037, "Long": -76.88685, "Description": "June: Railroad workers again rioted in the evening. One man killed, two others seriously injured. Some Gerrons and oe Native American attacked.", "Source": "NYEP, June 28, 1834", "Type": "Labor; Ethnic\/National; Racial", "Rioter Classification": "Worker; Railroad Worker", "Target Classification": "German; Native", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": 2, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.88685, 39.109037 ] } },
{ "type": "Feature", "properties": { "ID": 1114, "year": 1834, "date": "1834\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Authorities arrested a woman for stealing a pair of shoes. About 200 rioters shouting \"State Prison monopoly!\" attacked the shoe store, destroyed the awning, smashed the windows, and rescued some of the arrested rioters after the watch attempted to intervene. 2000 persons watched this scene.", "Source": "Weinbaum,M + D, p.66", "Type": "Law", "Rioter Classification": "Unknown", "Target Classification": "Merchant; Legal; Law Officer; Watchman", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1115, "year": 1834, "date": "1834\/07", "State": "Iowa", "Sublocation": "Dubuque", "Location": "Dubuque, Iowa", "Lat": 42.518756, "Long": -90.684951, "Description": "July: Mob whipped William Hoffman because his friend accused him of keeping the friend's money. Punishment stopped when friend found the money.", "Source": "Block. \"Lynchings…,\" Iowa JH+P, p.170-171", "Type": "Vigilante; Law", "Rioter Classification": "Vigilante", "Target Classification": "Alleged Criminal; Alleged Thief", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "William Hoffman", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.684951, 42.518756 ] } },
{ "type": "Feature", "properties": { "ID": 1116, "year": 1834, "date": "1834\/07", "State": "New Jersey", "Sublocation": "Camden", "Location": "Camden, NJ", "Lat": 39.944731, "Long": -75.116344, "Description": "July: Race riot concerning self-improvement. About 50 white people fought with some black people returning from a Sunday afternoon church service.", "Source": "Werner, p.273-239", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.116344, 39.944731 ] } },
{ "type": "Feature", "properties": { "ID": 1117, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Black eel-skinner beaten because he did his job cheaper than his fellow workers.", "Source": "Grinsted.Antebellum Labor, p.11", "Type": "Labor", "Rioter Classification": "Worker; White", "Target Classification": "Worker; Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1118, "year": 1834, "date": "1834\/07", "State": "Illinois", "Sublocation": "Illinois", "Location": "Illinois", "Lat": 40.608425, "Long": -89.05011, "Description": "July: More Mormon troubles.", "Source": "NYEP, July 9,17,25,1834", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Mormon", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -89.05011, 40.608425 ] } },
{ "type": "Feature", "properties": { "ID": 1119, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: A great mob of sailors attacked and pulled down James Moore's house at No. 6 James Slip.", "Source": "NYC GS SC PC - cases CC88 (7), People v. Thatcher et al., July 11, 1834", "Type": "Unknown", "Rioter Classification": "Sailor", "Target Classification": "Unknown", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "James Moore", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1120, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Disruption of anti-slavery meeting in Chatham street chapal.", "Source": "Weinbaum, p.38-39", "Type": "Slavery; Anti-Abolitionist", "Rioter Classification": "Anti-Abolitionist", "Target Classification": "Abolitionist; Anti-Slavery; Church; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1121, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Anti-abolitionists - July 7 disruption of black meeting in Chatham street chapel; July 8 disruption of Morally (??) meeting in Clinton hall; July 9 attack on L. Toppman's house, forced entry into Chatham st. chapel, riot at Bowery Theater; July 10, assault on Rev. Sam. Cox's house and church; July 11, attack on A. toppman and Co. store, Cox's church, Rev. Henry Ludlow's church; rampage against stp. black people.", "Source": "Richards, p.113-120", "Type": "Slavery; Anti-Abolitionist; Racial", "Rioter Classification": "Anti-Abolitionist; Pro-Slavery", "Target Classification": "Abolitionist; Anti-Slavery; Church; Black; Religious; Merchant", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "L. Toppman; Sam Cox; Henry Ludlow", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1122, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: After putting out a fire onPearl street, several firemen went down to the river to bathe. After that they climbed aboard the Herald. The captain told them to get off, believing that their nudity would offend female passengers. Blows were exchanged and there was a small affray.", "Source": "NYEP, July 9, 1834", "Type": "Firefighter", "Rioter Classification": "Firefighter", "Target Classification": "Sailor; Captain", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1123, "year": 1834, "date": "1834\/07", "State": "New Jersey", "Sublocation": "Newark", "Location": "Newark, NJ", "Lat": 40.733983, "Long": -74.172024, "Description": "July: Presbyterean minister preached a sermon on Friday evening entitled, \"Sin of Slavery,\" and planned to take a collection for the anti-slavery society. Large crowd formed outisde, some charge and drove minister and congregation out. Crowd then demolish the interior and smashed windows. They went to the minister's house but he wasn't there. Next night a mob attacked a black own barber shop. One black man had attended the meeting.", "Source": "Common Advertiser, July 16, 1834", "Type": "Slavery; Anti-Abolitionist; Racial; Religious", "Rioter Classification": "Anti-Abolitionist; Pro-Slavery", "Target Classification": "Presbyterian; Black; Religious; Church; Abolitionist; Merchant; Ant-Slavery", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.172024, 40.733983 ] } },
{ "type": "Feature", "properties": { "ID": 1124, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "Buffalo", "Location": "Bufallo, NY", "Lat": 42.880973, "Long": -78.845114, "Description": "July: At night at least a dozen black people rioted, attacking several houses including: the the house used by the Bd of Health for smallpox hospital and the house of a respectable Frenchman. Constable Furmen with friends quelled the disturbance. There was some resistance. One black man drowned when he tried to escape by jumping into the canal.", "Source": "Common Advertiser, July 25, 1834", "Type": "Racial", "Rioter Classification": "Black", "Target Classification": "White; French; Hospital", "Actions": "Property Destroyed", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -78.845114, 42.880973 ] } },
{ "type": "Feature", "properties": { "ID": 1125, "year": 1834, "date": "1834\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Seamen and boardinghouse keepers had agreed that no sailors should ship out across the Atlantic for less than $15 a month. Aat No.15 Burling slip a crew had signed on for $13 a month aboard the packet John Jay. A group of sailors threatened to strip the ship, if its papers were not surrendered. The captain said the papers were at the shipping office. Before the sailors could get there, police intervened and arrested 6 o 7.", "Source": "Common Advertiser, July 23, 1834", "Type": "Labor", "Rioter Classification": "Sailor", "Target Classification": "Sailor; Captain; Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1126, "year": 1834, "date": "1834\/08", "State": "New Jersey", "Sublocation": "Bloomfield", "Location": "Bloomfield, NJ", "Lat": 40.791803, "Long": -74.195897, "Description": "August: Mob of distinguished men cut in temperance cause. They attacked a tavern, destroyed its stock of liquor and then beat up the owner.", "Source": "Werner. \"Race Riots,\" p.42", "Type": "Temperance", "Rioter Classification": "Temperance; Reformer", "Target Classification": "Merchant; Tavern", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.195897, 40.791803 ] } },
{ "type": "Feature", "properties": { "ID": 1127, "year": 1834, "date": "1834\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: A gang of young men attacked Phoebe Doty's house, threatening her, etc. Two named.", "Source": "SC PC 7446 1, Doty v Granger et al., August 8, 1834", "Type": "Unknown", "Rioter Classification": "Youth", "Target Classification": "Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Phoebe Dotty", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1128, "year": 1834, "date": "1834\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: A gang of about 50 men assaulted son of James Forton, one of the wealthiest black residents in town.", "Source": "Werner, p.188", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "James Forton", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1129, "year": 1834, "date": "1834\/08", "State": "New York", "Sublocation": "Palmyra", "Location": "Palmyra, NY", "Lat": 43.062041, "Long": -77.232098, "Description": "August: Charles Williams, working as a clerk in Washington, returned hom to visit his mother. He brought an enslaved girl servant with him. Local black people talked her into running away. They also attacked Mrs. Williams house on Sunday breaking some windows. Shots exchanged and with the town alerted, a dozen black people were arrested.", "Source": "NYEP, August 18, 1834", "Type": "Slavery", "Rioter Classification": "Black; Anti-Slavery", "Target Classification": "White; Slaveholder; Woman", "Actions": "Threat; Property Destroyed; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Charles Williams; Williams", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.232098, 43.062041 ] } },
{ "type": "Feature", "properties": { "ID": 1130, "year": 1834, "date": "1834\/08", "State": "Massachusetts", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 42.350273, "Long": -71.060098, "Description": "August: Burning of Ursuline Convent. Large crowd outside, disperses, but then reassembles around midnight. 40-50 organized men burst in. Nuns and students escape and convent burnt. Next night mob of men and boys burn fences and trees and all else left on convent ground. Next night over 1,000 men roamed city but did little damage. On night of August 15 rioters burned a shanty in Charleston occupied by about 35 Irish laborers", "Source": "Billingston. \"Burning the Convent,\" p.4-24", "Type": "Religious; Ethnic\/National", "Rioter Classification": "Nativist; American; Youth", "Target Classification": "Catholic; Woman; Religious; Nun; Student; Irish; Convent", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 1131, "year": 1834, "date": "1834\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: White mob begins by attacking \"flying horses\" amusement establishment that had been used by black and white people. But then turned to assaults on black people and their property for three nights. Two churches and over 30 houses destroyed, damaged, or looted. One black person died from the wounds inflicted during beating. 100s flee. Authorities use militia, etc., to quell the riot along Philadelphia.", "Source": "Curray, p.106-7", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black; Religious; Church", "Actions": "Lethal Violence; Property Destroyed; Threat", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 1132, "year": 1834, "date": "1834\/08", "State": "New Jersey", "Sublocation": "Bloomfield", "Location": "Bloomfield, NJ", "Lat": 40.791803, "Long": -74.195897, "Description": "August: Saturday night 20 or 30 men \"with faces blackened, painted with various colors, or veiled\" entered William Iles' porter house, tied his hands and took him to a field. There they stripped him and treated him in a shameful manner. Others dumped the contents of his shop in the stream. Iles is an immigrant.", "Source": "NYEP, August 21, 1834", "Type": "Ethnic\/National", "Rioter Classification": "Nativist", "Target Classification": "Merchant", "Actions": "Seized Person(s); Property Destroyed; Humiliation", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "William Iles", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.195897, 40.791803 ] } },
{ "type": "Feature", "properties": { "ID": 1133, "year": 1834, "date": "1834\/08", "State": "Pennsylvania", "Sublocation": "Lancaster", "Location": "Lancaster, PA", "Lat": 40.031466, "Long": -76.281284, "Description": "August: Racial rioting. 1st two days minor skirmishes between black and white people. One 3rd day, black persons attacked a white man in an attempt to protect their property. About 30 white people then marched into the black section and attacked several houses and fired off some guns.", "Source": "NYEP, August 27, 1834", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.281284, 40.031466 ] } },
{ "type": "Feature", "properties": { "ID": 1134, "year": 1834, "date": "1834\/08", "State": "Pennsylvania", "Sublocation": "Columbia", "Location": "Columbia, Penn", "Lat": 40.033791, "Long": -76.499668, "Description": "August: For several nights whites break windows, etc, of black homes. On the 19th a crowd of 50 or 60 marched into the black area of town, stoned houses and fired guns.", "Source": "Werner. \"Columbia Race Riots,\" p.177-178", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.499668, 40.033791 ] } },
{ "type": "Feature", "properties": { "ID": 1135, "year": 1834, "date": "1834\/08", "State": "Pennsylvania", "Sublocation": "Columbia", "Location": "Columbia, Pa", "Lat": 40.033791, "Long": -76.499668, "Description": "August: Race riot concerning job competition, amalgamation fears and desire to drive black residents out of community. Windows broken by roving gangs of whites followed by attacks on black homes. Labor competition a big issue here. African Americans beaten in off-again on-again violence.", "Source": "Werner, p.198-202, 273", "Type": "Racial; Labor", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.499668, 40.033791 ] } },
{ "type": "Feature", "properties": { "ID": 1136, "year": 1834, "date": "1834\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Saturday night, about 12:30, about 20 persons started an uproar by shouting fire, etc., in Broadway. About 150 persons collecte and were disorderly for several hours. They stoned several houses including Nos. 22, 18, 10, and 2. Signs and windows were destroyed. When the watch arrived there was a scuffle; one watchman and one citizen were injured. The rioters didn't seem to have any objective and dispersed at 3:00am.", "Source": "NYEP, August 25, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Property Destroyed; Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": 2, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1137, "year": 1834, "date": "1834\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Sailors' strike in opposition to a reduction in wages.", "Source": "Commons J, p.478", "Type": "Labor", "Rioter Classification": "Sailor", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1138, "year": 1834, "date": "1834\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: A \"mob of persons\" attack Conrad Sebring's 102 Anthony street home, tearing down his privy and threatening him. Two were identified.", "Source": "SC PC 7446 13, Sebring Van Winkle et al., August 28, 1834", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Conrad Sebring", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1139, "year": 1834, "date": "1834\/09", "State": "Michigan", "Sublocation": "Detroit", "Location": "Detroit, Michigan", "Lat": 42.405935, "Long": -83.104235, "Description": "September: Minor race riot.", "Source": "Werner, p.273", "Type": "Racial", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -83.104235, 42.405935 ] } },
{ "type": "Feature", "properties": { "ID": 1140, "year": 1834, "date": "1834\/09", "State": "Michigan", "Sublocation": "Detroit", "Location": "Detroit", "Lat": 42.405935, "Long": -83.104235, "Description": "September: Mob tore down home of a black family that had somehow offended whites.", "Source": "Werner, p.239, 270", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -83.104235, 42.405935 ] } },
{ "type": "Feature", "properties": { "ID": 1141, "year": 1834, "date": "1834\/09", "State": "New Jersey", "Sublocation": "Trenton", "Location": "Trenton, NJ", "Lat": 40.216086, "Long": -74.757715, "Description": "September: Race riot, probably over self-improvement. Crowd outside African Methodist church then threw rocks and broke windows. Congregation fled.", "Source": "Werner, p.273, 178", "Type": "Racial; Religious", "Rioter Classification": "White", "Target Classification": "Black; Religious; Church; African Methodist", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.757715, 40.216086 ] } },
{ "type": "Feature", "properties": { "ID": 1142, "year": 1834, "date": "1834\/09", "State": "Conneticut", "Sublocation": "Cantebury", "Location": "Cantebury, CT", "Lat": 41.697139, "Long": -71.990091, "Description": "September: Mob physically dismantled Pedence Crandall's school.", "Source": "Richards, p.38-40", "Type": "Unknown", "Rioter Classification": "Unknown", "Target Classification": "School", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Pedence Crandall", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.990091, 41.697139 ] } },
{ "type": "Feature", "properties": { "ID": 1143, "year": 1834, "date": "1834\/09", "State": "Pennsylvania", "Sublocation": "Columbia", "Location": "Columbia, Penn", "Lat": 40.033791, "Long": -76.499668, "Description": "September: Crowd of white workers destroy property - porch, windows, entered house and rifled papers - of affluent black people - especially lumber merchant Stephen Smith.", "Source": "Slaughter.Bloody Dawn, p.174-175", "Type": "Racial", "Rioter Classification": "White; Worker", "Target Classification": "Black; Merchant", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Stephen Smith", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.499668, 40.033791 ] } },
{ "type": "Feature", "properties": { "ID": 1144, "year": 1834, "date": "1834\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Carpet weavers strike in opposition to a reduction in wages.", "Source": "Commons I, p.479; National Traders' Union, September 6, 1834", "Type": "Labor", "Rioter Classification": "Worker; Weaver", "Target Classification": "Employer", "Actions": "Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1145, "year": 1834, "date": "1834\/09", "State": "South Carolina", "Sublocation": "South Carolina", "Location": "South Carolina", "Lat": 33.93198, "Long": -80.507899, "Description": "September: Affray between union men and nullifiers. Union men head off a nullifying county meeting. Blows were exchanged.", "Source": "NYEP, September 12, 1834", "Type": "Political", "Rioter Classification": "Political; Unionist", "Target Classification": "Political; Nullifier", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -80.507899, 33.93198 ] } },
{ "type": "Feature", "properties": { "ID": 1146, "year": 1834, "date": "1834\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: A rumor spread that a \"respectable\" girl had been seduced and then, when she came down with cholera, turned out to die by a brothel. 200 to 300 persons marched on the place and, encouraged by the rest about a dozen entered and destroyed the premises.", "Source": "Weinbaum.M+D, p.66", "Type": "Moral Panic; Vigilante", "Rioter Classification": "Vigilante", "Target Classification": "Brothel; Woman; Prostitute", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1147, "year": 1834, "date": "1834\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Every black person on the Battery was compelled by a mob to run for shelter in the nearest building. Few black people showed up the next day. In the afternoon, the rioters numbered \"hundreds\". Most were young men about 20 years old.", "Source": "NYEP, September 25, 1834", "Type": "Racial", "Rioter Classification": "Youth; White", "Target Classification": "Black", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1148, "year": 1834, "date": "1834\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: In the afternoon a barouche was attacked by a mob because the servant driver was black. A lady and her daughter in the coach were in danger.", "Source": "NYEP, September 25, 1834", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Woman; White; Black; Worker; Driver", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1149, "year": 1834, "date": "1834\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Irishmen and nativists fought one another. In Sept. there was a brawl involving almost 1,000 persons. Some of the fighting was between Protestant and Catholic Irish.", "Source": "Weinbaum.M+D, p.55,66", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; Protestant; Nativist; American", "Target Classification": "Irish; Catholic; Protestant; Nativist; American", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1150, "year": 1834, "date": "1834\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Large crowd gathered at the Battery to watch a balloon ascension; amused themselves by taunting and beating some African Americans in the crowd.", "Source": "Werner, p.178", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1151, "year": 1834, "date": "1834\/10", "State": "Pennsylvania", "Sublocation": "Columbia", "Location": "Columbia, Penn", "Lat": 40.033791, "Long": -76.499668, "Description": "October: White crowd attacks homes of four property belonging to African Americans. Stealing and looting proprerty for at least one and destroying property. One black person injured.", "Source": "Slaughter.Bloody Dawn, p.176-177", "Type": "Racial", "Rioter Classification": "White", "Target Classification": "Black", "Actions": "Seized Property; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.499668, 40.033791 ] } },
