{ "type": "Feature", "properties": { "ID": 1610, "year": 1853, "date": "1853\/12", "State": "Ohio", "Sublocation": "Cincinnati", "Location": "Cincinnati", "Lat": 39.105032, "Long": -84.517971, "Description": "December: Printers throw out some of the type of the Cincinnati Gazette when it failed to meet workers' demands, unlike other city papers.", "Source": "Grimsted. \"Antebellum Labor,\" p.11", "Type": "Labor", "Rioter Classification": "Worker", "Target Classification": "Press", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -84.517971, 39.105032 ] } },
{ "type": "Feature", "properties": { "ID": 1690, "year": 1855, "date": "1855\/01", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: Longshoreman's strike. An Irishman attacked a black man who wanted to work. Most African Americans had been intimidated off of the docks by the Irish Longshoreman's society. Armed, the black men fired at the Irishmen. Hundreds of other strikers immediately appeared. But the black man managed to escape.", "Source": "Bernstein, p.119-210", "Type": "Labor; Racial", "Rioter Classification": "Irish; Black; Worker; Longshoreman; Striker; Strikebreaker", "Target Classification": "Irish; Black; Worker; Longshoreman; Striker; Strikebreaker; Employer", "Actions": "Strike; Lethal Violence; Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 1827, "year": 1857, "date": "1857\/05", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "May: Striking RR workers use violence to prevent trains from running. On the 1st they ambushed a train leaving Baltimore at a deep cuty at Jackson's bridge. Shots were exchanged with police. One train got through, three others did not. A similar confrontation occurred Sunday. Several strikers wounded both days, and on the 2nd one was killed. The rioters also spiked tracks. After 3rd some settlement was reached. Slight to workers.", "Source": "Scharf.Chronicles, p.555-557", "Type": "Labor; Law", "Rioter Classification": "Striker; Worker; Railroad Worker", "Target Classification": "Railroad", "Actions": "Lethal Violence; Property Destroyed; Strike", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
