{ "type": "Feature", "properties": { "ID": 1530, "year": 1851, "date": "1851\/01", "State": "Virginia", "Sublocation": "Culpepper County", "Location": "Culpepper County, VA", "Lat": 38.47631, "Long": -77.99381, "Description": "January: At Hazel River Works a riot between white and black workers left one black person (?). The whites later fought among themselves.", "Source": "Wesley.Negro Labor, p.79", "Type": "Racial; Labor", "Rioter Classification": "White; Worker", "Target Classification": "White; Black; Worker", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.99381, 38.47631 ] } },
