{ "type": "Feature", "properties": { "ID": 1323, "year": 1842, "date": "1842\/07", "State": "Pennsylvania", "Sublocation": "Wadesville", "Location": "Wadesville, PA", "Lat": 40.71609, "Long": -76.20651, "Description": "July: Striking miners compelled others to join them. Sheriff intervened and arrested a few ringleaders.", "Source": "Palladino.Another Civil War, p.50", "Type": "Labor", "Rioter Classification": "Worker; Striker; Miner", "Target Classification": "Worker", "Actions": "Strike; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.20651, 40.71609 ] } },
