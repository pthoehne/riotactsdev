{ "type": "Feature", "properties": { "ID": 1508, "year": 1850, "date": "1850\/07", "State": "New York", "Sublocation": "NYC", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Striking tailors attack priest and a church because the priest distributed work to his tailor parishoners.", "Source": "Degler. \"Labor,\" p.81; Cook. \"Armies,\" p.21", "Type": "Labor; Religious", "Rioter Classification": "Tailor; Worker; Striker", "Target Classification": "Employer; Worker; Tailor; Strikebreaker; Religious; Catholic; Church; Priest", "Actions": "Non-Lethal Violence; Threat; Strike", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
