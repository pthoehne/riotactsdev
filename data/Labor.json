{ "type": "Feature", "properties": { "ID": 1445, "year": 1848, "date": "1848\/07", "State": "Pennsylvania", "Sublocation": "Allegheny City, PA", "Location": "Allegheny City, PA", "Lat": 40.44685, "Long": -80.01691, "Description": "July: Strikers broke some machinery in a cotton mill when some of their coworkers went back to work. Hours one of the key issues in strike. Women participate. Hours also an issue. Riot began in earnest after owner opened steam valve on strikers.", "Source": "Grimsted. \"Antebellum Labor,\" p.10,22; Adamic.Dynamite, p.6-7", "Type": "Labor", "Rioter Classification": "Worker; Woman; Striker", "Target Classification": "Employer; Strikebreaker", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -80.01691, 40.44685 ] } },
