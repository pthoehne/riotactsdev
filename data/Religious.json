{ "type": "Feature", "properties": { "ID": 2150, "year": 1863, "date": "1863\/07", "State": "New York", "Sublocation": "Troy", "Location": "Troy, NY", "Lat": 42.731883, "Long": -73.686965, "Description": "July: Draft riot - 300 to 400 laborers from local foundry marched through town, demanding an end to the draft, stoned andd gutted the office of the Troy times, threatened a black church and released 80 prisoners from jail. Several houses gutted, too.", "Source": "Coakley.Role, p.256-257", "Type": "Military; War; Draft; Racial; Religious", "Rioter Classification": "Anti-Draft; White", "Target Classification": "Black; Press; Religious; Church; Legal; Jail", "Actions": "Threat; Property Destroyed; Extraction", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.686965, 42.731883 ] } },
