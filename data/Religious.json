"features": [
{ "type": "Feature", "properties": { "ID": 25, "year": 1784, "date": "1784\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: \"Several riots having happened at the door of the Methodist meeting, to the great annoyance, and vexation of the pious and well-inclined, Mr. Aldermon (??) Lott attended on Sunday evening, for the (??) purpose of enforcing a proper discourse, when several bucks beginning to beat up a dust, the ring leaders were taken into custody, and confined for the night in the Provost.\"", "Source": "N.Y. Gazetteer, November 30, 1784", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Aldermon Lott", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 30, "year": 1785, "date": "1785\/04", "State": "Virginia", "Sublocation": "Unknown", "Location": "Virginia", "Lat": 37.501713, "Long": -78.033621, "Description": "April: Thomas Coke, a Methodist preacher, was threatened with mob violence and had two bills of indictment present against him for speaking out against slavery in the spring of 1785.", "Source": "Geasche(??). The Great Awakening in Virginia,p.247", "Type": "Anti-Abolition; Religious; Slavery", "Rioter Classification": "Anti-Abolitionist", "Target Classification": "Abolitionist; Religious; Methodist", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Thomas Coke", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -78.033621, 37.501713 ] } },
{ "type": "Feature", "properties": { "ID": 32, "year": 1785, "date": "1785\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: British sailors and Tory sympathizer attacked members of a French packet. This aroused the populace and brought forth legislative action. Rioters include (convicted and sentenced) Jeremiah Watson, Joseph Biggs, and James Watson.", "Source": "Pomerantz. p.298", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "Sailor; Political; British; Tory", "Target Classification": "French; Catholic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": "Jeremiah Watson; Joseph Biggs; James Watson", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 41, "year": 1785, "date": "1785\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: A \"fracas\" occurred between the N.E. ship Friendshipand the Marie Victorie, a French vessel, at the old Ferry Wharf. It occurred at 2:00pm and was triggered over and argument concerning an open slot at the wharf. The French ship had arrived first and N.E. crew, sans the captain, attacked the French twice. The N.E. crew numbered 12 or 14 men. Four of them were afterwards arrested but only three were arraigned to be tried at the September session of the court of O+T.", "Source": "N.Y. Journal, August 18, 1785.N.Y. Packet, August 18, 1785.", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Sailor; American; Protestant", "Target Classification": "Sailor; French; Catholic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 93, "year": 1787, "date": "1787\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Dr. Carrol, American Catholic religious leader came to NYC and founed Andrew Nugent, pastor of the Catholic congregation there. He decided that Nugent was misguided and annulled his faculties. Nugent opposed this and refused to yield possession of St. Peters. The matter was placed before the Grand Jury who found Nugent guilty of riot. A new pastor was appointed by Carrol.", "Source": "Shea, ed.Catholic Churches of NYC, p.591; Bennett. Catholic Footsteps…, p.381-383;N.Y. Journal, N.19, 1787 - oblique reference", "Type": "Religious", "Rioter Classification": "Religious; Catholic", "Target Classification": "Religious; Catholic", "Actions": "Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Andrew Nugent", "Persons Named as Targets": "Carrol", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 117, "year": 1789, "date": "1789\/02", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "February: Bluet. Evangelist John Marrant describes how he was harassed by a mob of about 40 who interrupted his preaching in the West end of Boston. Marrant escaped immediate harm, returned to his home, followed by the crowd who then pelted rocks on his house.", "Source": "Brooks. \"Emergence of a Black Print Counterpublic,\"WMQ, 3rd ser., 62 (2005), 76", "Type": "Racial; Religious", "Rioter Classification": "White Supremacist", "Target Classification": "Black; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "John Marrant", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 119, "year": 1789, "date": "1789\/12", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "December: Dispute over who was the priest in the one Catholic congregation led one group of malcontents to disrupt services, overturn pews and generally misbehave.", "Source": "Coghimo.No King, No Pope, (??) p.135-136", "Type": "Religious", "Rioter Classification": "Religious; Catholic", "Target Classification": "Religious; Catholic", "Actions": "Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 132, "year": 1791, "date": "1791\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Possible rioting around Easter. It might be connected to the holiday or it might be related to the election. The source is very unclear.", "Source": "N.Y. Journal, January 2, 1791", "Type": "Political; Religious", "Rioter Classification": "Political; Religious", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 155, "year": 1793, "date": "1793\/05", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "May: Apparently there were a number of riotous scuffles between English and French sailors in this port. It even drew a comment from the British consul there requesting English sailors to keep the peace and a special charge to the Philadelphia Grand Jury.", "Source": "N.Y. Daily Advertiser, June 13, 1793", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; British; French; Protestant; Catholic", "Target Classification": "Sailor; British; French; Protestant; Catholic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 158, "year": 1793, "date": "1793\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: A fight occurred between French and English sailors near Dover St.; also at the lower end of King St. Some bloodwas spilled as the combatants were armed with clubs. The watch took off several of them from the house in which the quarrel arose.", "Source": "Alexander Anderson.Diarium…, August 18, 1793;N.Y. Journal, August 21, 1793", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; British; French; Protestant; Catholic", "Target Classification": "Sailor; British; French; Protestant; Catholic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 180, "year": 1794, "date": "1794\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Sailors on a Portuguese Brig, in honor of Good Friday, hanged Judas in effigy from the ship. The effigy was dunked in the water, before being dragged through the streets while beaten with clubs. Provided amusement to boys and spectators.", "Source": "Gazette of the United States & evening advertiser. [volume] (Philadelphia [Pa.]), 22 April 1794. Chronicling America: Historic American Newspapers. Lib. of Congress.", "Type": "Religious", "Rioter Classification": "Sailor; Portuguese; Youth; American", "Target Classification": "Religious", "Actions": "Effigy", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Judas", "Note": "PTH", "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 248, "year": 1796, "date": "1796\/07", "State": "North Carolina", "Sublocation": "Wilmington", "Location": "Wilmington, NC", "Lat": 34.218719, "Long": -77.931798, "Description": "July: French privateer La Bellona came into port. The captain and crew had several run-ins with the citizens and American seamen of the town. These culminated in a \"sailor's brawl\" (Gilpatrick's phrase) in which an American seaman was killed, the privateer guns thrown overboard, and the French tricolor trampled undefoot. The Americans believed that the privateer was enlisting American seamen and planning to molest American shipping leaving Wilmington. There was a trial of the French privateer afterward.", "Source": "Gilpatrick.Jeffersonian Democracy in N.C., p.83", "Type": "Political; Ethnic\/National; Religious; Economic", "Rioter Classification": "Sailor; French; Catholic; Vigilante; Protestant", "Target Classification": "Sailor; French; Catholic; Vigilante; Protestant", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.931798, 34.218719 ] } },
{ "type": "Feature", "properties": { "ID": 284, "year": 1798, "date": "1798\/08", "State": "Georgia", "Sublocation": "Savannah", "Location": "Savannah", "Lat": 32.082669, "Long": -81.110266, "Description": "August: The crew of a Spanish privateer was harassed and the vessel taken up river and burnt by a number of men about midnight. The Spanish vessel had taken two American ships which the Spanish government had later released.", "Source": "NY.Time Piece, August 30, 1798", "Type": "Ethnic\/National; Political; Religious; Vigilante; Law", "Rioter Classification": "Political; Vigilante", "Target Classification": "Sailor; Spanish; Catholic; Political; Privateer; Alleged Criminal; Hispanic", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -81.110266, 32.082669 ] } },
{ "type": "Feature", "properties": { "ID": 294, "year": 1799, "date": "1799\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Effigy paddies cause disturbance on day of celebration for St. Patrick in Harmon Street. One man was killed.", "Source": "N.Y. Argus, March 20, 1799", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Unknown", "Target Classification": "Irish; Religious; Catholic", "Actions": "Effigy; Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 325, "year": 1800, "date": "1800\/10", "State": "Virginia", "Sublocation": "Great Bridge", "Location": "Great Bridge, VA", "Lat": 36.71319, "Long": -76.23981, "Description": "October: A Baptist preacher and Republican, Davis Biggs, was invited to preach at Great Bridge in Norfolk County. A Mr. W Butt or Burt, a local magistrate and Federalist, attempted to gather men to tar and feather the preacher. If he failed to find enough men, he threatened to cut off Bigg's ears, let out his bowels, or send him to Hell, \"where all such apostates should be sent, when found.\"", "Source": "The national intelligencer and Washington advertiser. [volume], December 12, 1800, Image 4", "Type": "Political; Religious; Law", "Rioter Classification": "Political; Federalist; Legal; Judge; Magistrate", "Target Classification": "Religious; Baptist; Democratic-Republican", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": "W. Butt", "Persons Named as Targets": "Davis Biggs", "Note": "PTH", "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.23981, 36.71319 ] } },
{ "type": "Feature", "properties": { "ID": 334, "year": 1802, "date": "1802\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: A number of Irishmen collected about sunset on the 17th on East George street and harassed nearly everyone who walked by. They attacked a number of people with clubs and stones and shouted \"Huzzah for Dublin, we'll show the Americans freedom.\" Seven were arrested while there was at least 20 others.", "Source": "NYC General Sessions April 16, 1802; MMPO II March 17 and 18, 1802", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Unknown", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 345, "year": 1804, "date": "1804\/09", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "September: \"September witnessed a riot begun by several Spanish sailors, who stabbed and dangerously wounded William Barry, a young American sailor. His friends went to a Spanish house and tore it to pieces. Several people were severely hurt during the riot.\"", "Source": "Scharf and Wiscott.History of Philadelphia,P.519", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Spanish; Catholic; Sailor; American; Protestant; Hispanic", "Target Classification": "Spanish; Catholic; Sailor; American; Protestant; Hispanic", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "William Barry", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 346, "year": 1804, "date": "1804\/09", "State": "Maryland", "Sublocation": "Elkton", "Location": "Elkton, Md", "Lat": 39.609286, "Long": -75.825092, "Description": "September: A number of united Irishmen, canal workers, left off work, went to the race track armed with bludgeons and (??). There they paraded about and became involved in a fight with some blacks. They demolished one free black person's stall and chased the rest of the blacks toward town. The blacks bought protection from the whites. Several gentlemen of the races attempted to intercede. The Irish attacked these gentlemen too and a battle ensued between this mounted gentry armed with whips and Irish ruffians. Some of the gentry were injured, but it is inplied that the rioters got the worst of it, a few of whom, it was said, died afterwards. On their approach to the races the Irish are reported as declaring they could whip double their number of Americans.", "Source": "N.Y.E.P., October 13, 1804", "Type": "Racial; Labor; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Black; Protestant; American", "Actions": "Lethal Violence; Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.825092, 39.609286 ] } },
{ "type": "Feature", "properties": { "ID": 349, "year": 1805, "date": "1805\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: A fracas occurred at Bowling Green. Some Irishmen commented loudly that no American soldier was worth looking at except the Irish Greens. A yankee objected and a fist fight ensued which quickly expanded into a free-for-all. The mayor attempted to intercede but was at first unsuccessful. Some say he himself was knocked down and his stick taken. He rallied, however, and with the assistance of some other gentlemen and the constables the fight was stopped and three or four of the ringleaders were sent off to Bridewell.", "Source": "NYEP, July 6, 1805", "Type": "Ethnic\/National; Political; Religious; Vigilante", "Rioter Classification": "Irish; Catholic; American; Protestant", "Target Classification": "Irish; Catholic; American; Protestant", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 362, "year": 1806, "date": "1806\/10", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "October: A fracas occurred between the crew of a French frigate and a party of American sailors in Southwark at night.", "Source": "NYEP, October 21, 1806", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; French; Catholic; American; Protestant", "Target Classification": "Sailor; French; Catholic; American; Protestant", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 363, "year": 1806, "date": "1806\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Highbinders riot and Irish riot on Augustus street. Highbinders had a record of attacking whore houses and committing other disorderly acts. They had congregated on the night of the 24th to disrupt possible Catholic services at the B.C. Church at Barclay St. No real trouble occurred except abusing of watchmen. Following night the Irish gathered to protect their religion. The watch attempted to break up the Irish crowd and riot and battle ensued. De Witt Clinton gov. offered reward for ringleader and murderer of watchmen.", "Source": "Matteson MSS - Box 3 and notecards; NYEvening Post, December 26, 1806", "Type": "Gang; Ethnic\/National; Religious; Moral Panic", "Rioter Classification": "Highbinder; Protestant; Nativist; Irish; Catholic; Gang", "Target Classification": "Highbinder; Protestant; Nativist; Irish; Catholic; Gang; Woman; Prostitute", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": "2", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 367, "year": 1807, "date": "1807\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Trustors of African American church complain their church being disturbed by boys and want watchmen to protect them.", "Source": "MCC, 4, p.389", "Type": "Racial; Religious", "Rioter Classification": "Youth; White", "Target Classification": "Black; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 370, "year": 1807, "date": "1807\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Trustees of the Methodist Episcopal Church complain \"of the interruption of their service on Sundays by disorderly persons\" and request a watchmen to protect them.", "Source": "MCC4: p.396, 407", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 373, "year": 1807, "date": "1807\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: In the aftermath of the Chesapeake affair, Augustus Foster, British Secretary of Legation decided to travel incognito and trusted his horses to the separate care of his groom. This proved to be a wise decision when only the presence \"of a somewhat timid soul\" counseling moderation prevented the curricle and horses from being thrown into the Norte River. Foster commented that the ringleader on this occasion, as might be expected, an Irish emigrant.", "Source": "Horseman.Causes…, p.103", "Type": "Ethnic\/National; Religious", "Rioter Classification": "American; Irish; Catholic", "Target Classification": "British", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "Augustus Foster", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 378, "year": 1807, "date": "1807\/08", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "August: Some Frenchmen, in an armed boat fitted out in Baltimore, boarded and captured the ship Othello from Liverpool. Contrary winds forced them to abandon their prize to the captain. Some militia set out to capture these \"pirates\" but questions over jurisdiction in the case prevented prosecution. Subsequently an act of the assembly provided a law to cover this situation.", "Source": "T.W. Griffith.Annals of Baltimore, p.180-187", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "Sailor; French; Catholic", "Target Classification": "British; Militia", "Actions": "Seized Property", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 384, "year": 1807, "date": "1807\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Samuel Stillwell, on behalf of the cities' Methodists, petitioned Mayor Willett in regard to violence and insults committed by rowdies and crowds on Sunday against Methodist churches.", "Source": "Stokes.IconographyV, p.1474", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Samuel Stillwell", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 401, "year": 1808, "date": "1808\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Methodists were frequently harassed on their way to and from church as well as at their services. On the night of the 11th Moses Flannagan with at least ten others again did so. This in the 10th ward. Flannagan was convicted.", "Source": "NYC General Sessions, October 8, 1808", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": "Moses Flannagan", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 403, "year": 1808, "date": "1808\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: The Trustees of the African Methodist church complain of riots at their place of worship and against Duncan McDonald, a watchman, for neglect of duty.", "Source": "MCC5: 272, 278", "Type": "Racial; Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Black; Methodist", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 426, "year": 1809, "date": "1809\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: \"The Church of Christ\" in the 8th ward was harassed by a large crowd who thre stones and balls made of bow and turpentine.", "Source": "SC -cases 7432", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 428, "year": 1809, "date": "1809\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: John Edward's religious group was bothered by a crowd in the 8th ward.", "Source": "SCPC - Cases 7432", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "John Edward", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 431, "year": 1809, "date": "1809\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Lozici, a marshal, was charged with rioting and disturbing Amos Broad's makeshift Baptist Society church on corner of Anthony and Handson streets.", "Source": "NYC General Sessions, Feb 10, 1810", "Type": "Religious; Slavery", "Rioter Classification": "Legal; Marshal; Anti-Slavery; Law Officer", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Lozici", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 433, "year": 1810, "date": "1810\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Amos Broad again being harassed. Accuses Peter Von Zandt, scion of a wealthy family with others for disturbing his services. Result: NG.", "Source": "NYC General Sessions, April 13, 1810", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Peter Von Zandt", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 434, "year": 1810, "date": "1810\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Religious meeting - corner of Hudson and Anthony street, harassed by a disorderly crowd.", "Source": "SC PC - cases 7432 4", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 435, "year": 1810, "date": "1810\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Amos Broad's service was disrupted by a number of persons who created a riot in and about the church.", "Source": "SC PC cases 7432 5", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat; Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 436, "year": 1810, "date": "1810\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Miln Parker, harassed Amos Broad's Baptist church - stamping his feet, laughing and disturbing the congregation.", "Source": "SC PC - cases 7432 12", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Miln Parker", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 437, "year": 1810, "date": "1810\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Amos Broad is on Rose street. He was harassed by Bloodgood and five others. This time he got a conviction.", "Source": "NYC General Sessions, April 13, 1810", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": "Bloodgood", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 438, "year": 1810, "date": "1810\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Henry Diedrick with five or more others rioted and disturbed a church in the 4th ward.", "Source": "NYC General Sessions, June 15, 1810", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Local", "Persons Named as Rioters": "Henry Diedrick", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 440, "year": 1810, "date": "1810\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Broad's church harassed during the evening service. A number of young people's entered the church, put out the candles, carried away the candlesticks, broke the pews, the door, and disturbed Broad and et.al.", "Source": "SC PC cases - 7432 9", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Occupied Space; Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 444, "year": 1810, "date": "1810\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: A large disturbance at Broad's church. The Marshals were there at the order of the mayor in anticipation of the riot and several arrests were made. As many people were throwing squibs - quantities of gun powder enclosed in paper - at the church etc.", "Source": "SC PC - cases 7432 7", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 448, "year": 1810, "date": "1810\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Revivalists' meetings in the park were seen as \"disorderly assemblies\" and the Common Council passed a law otlawing them on July 2, 1810.", "Source": "Stokes.Iconography…V, p.1522", "Type": "Religious", "Rioter Classification": "Religious; Revivalist", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 453, "year": 1810, "date": "1810\/09", "State": "Ohio", "Sublocation": "Warren Co.", "Location": "Warren Co., Ohio", "Lat": 39.433938, "Long": -84.21105, "Description": "September: The Shaker community at Turtle Creek was harassed by upwards of a thousand persons from Warren and Butler counties. Their number included several milita companies and many private citizens. They had collected there to compel the Shakers to deliver up the grandchildren of Col. James Smith of Kentucky. The children, it was said, were being held there by force. A conference was held in which the Shakers declared that the children were with their father at Lebanon Jail. Not finding anyone desirous of leaving the multitude dispersed. However, they had warned the Shakers that if their secret were not broken up by the 1st of Nov, next they would receive another visit. No property and persons were damaged.", "Source": "NYEPSeptember 12, 1810", "Type": "Religious", "Rioter Classification": "Milita; Private Citizen", "Target Classification": "Religious; Shaker", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Regional", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -84.21105, 39.433938 ] } },
{ "type": "Feature", "properties": { "ID": 456, "year": 1810, "date": "1810\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Amos Broad is again disturbed in his ministry. This time by Betty, an ex-slave of his. Her actions incited farther mob action such as throwing of stones at window, etc. Result: guilty", "Source": "NYC General Sessions, October 15, 1810", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 457, "year": 1810, "date": "1810\/12", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "December: A constable came aboard a NY ship and demanded with a warranted written in French the custody of one crew member - a black man. The captain refused and insisted that the constable get an English warrant. This the constable refused to do. The constable even went and got a written note from the mayor saying that the warrant was valid. Still, the captain remaind steadfast and while he went himself to discuss the issue with the mayor the constable at the head of nine or ten of the city guard and with sabres drawn, attempted to board the vessel. This was opposed by the sailors who armed with handspikes drove them off. The captain arrived to help quell the disturbance. One sailor was dangerously wounded, while several of the guard were hurt.", "Source": "NY Gazette, January 24, 1811", "Type": "Racial; Law; Ethnic\/National; Religious", "Rioter Classification": "Sailor; American; Black", "Target Classification": "French; Catholic; Legal; Law Officer; Constable", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 458, "year": 1811, "date": "1811\/11", "State": "Georgia", "Sublocation": "Savannah", "Location": "Savannah", "Lat": 32.082669, "Long": -81.110266, "Description": "November: Savannah mob of sailors, spurred by merchants, set the torch to La Franchise and La Vengeance after repeated gun and knife battle which took one American and seven or eight French lives.", "Source": "Browt.MadisonV, p.376", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; American; Merchant", "Target Classification": "Sailor; French; Catholic", "Actions": "Lethal Violence", "Recorded # Killed": "9", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -81.110266, 32.082669 ] } },
{ "type": "Feature", "properties": { "ID": 459, "year": 1811, "date": "1811\/11", "State": "Georgia", "Sublocation": "Savannah", "Location": "Savannah", "Lat": 32.082669, "Long": -81.110266, "Description": "November: \"a riot took place between French privateers or English sailors; several men on both sides were killed. The privateers were burned and Seurier complained in language such as Napolean might be supposed to expect from his minister in regard to a violent onfragcot the French flag.\" It was something of a diplomatic incident and Madison knew about it.", "Source": "H Adams.History…VI, p.186-187", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "British; Sailor; Protestant", "Target Classification": "French; Sailor; Catholic; Privateer", "Actions": "Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -81.110266, 32.082669 ] } },
{ "type": "Feature", "properties": { "ID": 466, "year": 1811, "date": "1811\/06", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "June: French captain of a privateer stoned during heat of French activity against American shipping.", "Source": "Browt.Madison…V, p.315", "Type": "Political; Ethnic\/National; Religious; Economic; Vigilante", "Rioter Classification": "American", "Target Classification": "French; Sailor; Officer; Catholic; Privateer", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 472, "year": 1811, "date": "1811\/09", "State": "South Carolina", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "September: After a quarrel with William Chambers, grocer, several French and Spanish sailors were forced to retreat Sunday night. The next day, however, they returned armed with knives and clubs, and reinforced its members with more members of the crew of the French privateer La Vegeance. In the ensuing affray, Williams was murdered and several others wounded. Four of the privateers were arrested while a fifth Jon Chocolate, believed to be the murderer, was picked up the next day. One American sea captain was also severely wounded.", "Source": "NYEP, September 16, 1811", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "Sailor; French; Spanish; Catholic; Privateer", "Target Classification": "Merchant; American", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "William Chambers", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 473, "year": 1811, "date": "1811\/11", "State": "Georgia", "Sublocation": "Savannah", "Location": "Savannah, Ga", "Lat": 32.082669, "Long": -81.110266, "Description": "November: Riot between American and French seamen. Burning of French privateers, militia called out, but abandoned vessel rather than fire on their own countrymen.", "Source": "Matteson - MS - notecards + Box 3", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "Sailor; American", "Target Classification": "Sailor; French; Catholic; Privateer", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -81.110266, 32.082669 ] } },
{ "type": "Feature", "properties": { "ID": 477, "year": 1811, "date": "1811\/12", "State": "South Carolina", "Sublocation": "Charleston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "December: An attempt was made to burn the privateer brig Dilligent, but on the representation of the French consul the mayor caused the ringleader to be apprehended and jailed.", "Source": "NYEP, December 16, 1811", "Type": "Vigilante; Ethnic\/National; Religious", "Rioter Classification": "Vigilante", "Target Classification": "French; Catholic; Sailor; Privateer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 479, "year": 1812, "date": "1812\/01", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "January: Several ships were detained by the ice at (??) Point. Two females from the city were going to visit a Portuguese or Spanish ship lying outside the schooner Liberty. While crossing the Liberty's deck they were ordered not to pass that way - these orders were followed by an order to paint their faces. One sailor did this using a tar brush. Seeing this the foreigners streamed aboard, a fight followed in which several men were injured.", "Source": "NYEP, January 13, 1812", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; Spanish; Portuguese; American; Catholic", "Target Classification": "Woman; Sailor; Spanish; Portuguese; Catholic; American", "Actions": "Non-Lethal Violence; Humiliation", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 480, "year": 1812, "date": "1812\/02", "State": "Louisiana", "Sublocation": "Baton Rouge", "Location": "Baton Rouge", "Lat": 30.442973, "Long": -91.181329, "Description": "February: A number of Frenchmen were dissatisfied with a ruling of the court. They collected together and went to the courthouse in search of the judge. Not finding him there they dispersed at the urging of Mr. Baldwin who had his office in the courthouse. The next day the rioters returned. Again the Judge was not to be found. This time they vented their anger upon Baldwin, seized the Ct. house, and armed themselves with the cache of weapons stored there. The Sheriff couldn't dislodge them until he got some military assistance. However, no blood was spilt.", "Source": "NYEP, March 5, 1812", "Type": "Ethnic\/National; Law; Religious", "Rioter Classification": "French; Catholic", "Target Classification": "Legal; Judge; Sheriff; Law Officer", "Actions": "Seized Person(s); Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Baldwin", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -91.181329, 30.442973 ] } },
{ "type": "Feature", "properties": { "ID": 495, "year": 1812, "date": "1812\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: John Silva attacked by at least 10 men. Silva was Portuguese.", "Source": "NYC General Sessions July 8, 13, 1812", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Unknown", "Target Classification": "Portuguese; Catholic; Hispanic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "John Silva", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 508, "year": 1812, "date": "1812\/10", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "October: Amos Broad and his church are again being mobbed by 20 or 50. The one indicted is acquitted. Thompson, the accused, was inside disturbing the ceremony while a mob raged and threatened to come in from the outside.", "Source": "NYC General Sessions, November 5, 1812", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 510, "year": 1812, "date": "1812\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Amos Broad's church still being harassed. With the support of another witness Broad gets one conviction as he put up with 40 or 50 persons running through his church.", "Source": "NYC General Sessions, Dcember 18, 1812", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 511, "year": 1812, "date": "1812\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Broad's church was the object of an unruly crowd of men and boys in the evening. Jacob Hays was there and arrested several persons. Hays says there were a great number of boys and men.", "Source": "SC PC - cases 7432 3", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery; Youth", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 513, "year": 1812, "date": "1812\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Catherine Lowe schoolhouse run by George Mills was disrupted early in the morning by about 12 to 14 Irishmen who broke doors, threatened etc.", "Source": "SC PC - cases 7432 4", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "School", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "George Mills", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 520, "year": 1813, "date": "1813\/02", "State": "Virginia", "Sublocation": "Norfolk", "Location": "Norfolk", "Lat": 36.855595, "Long": -76.298084, "Description": "February: On the night of the 5th there was a fracas between Spanish and American sailors in which the Americans got the upper hand. The following evening, Saturday, 20 or 30 Spaniards returned for revenge armed with knives. Provoking the Americans into a fight in Water street they surprised the unarmed Americans. Within a few minutes six Americans were stabbed, one dying on the spot. Soon however the guard arrived and secured the greater part of the murders rescuing them from the enraged populace. The Spaniards were taken to the prison.", "Source": "NYEP, February 16, 1813", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; Spanish; Catholic; Hispanic", "Target Classification": "Sailor; American", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": 5, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.298084, 36.855595 ] } },
{ "type": "Feature", "properties": { "ID": 524, "year": 1813, "date": "1813\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: In the evening about 10 or 11 o'clock, a great riot occurred at the corner of East George and Henry street. Between 30 or 40 men, who appeared to be sailors, were there. A Spaniard by the name of Manuel Prede was beaten.", "Source": "SC PC cases - 7433 5", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor", "Target Classification": "Sailor; Spanish; Catholic; Hispanic", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Manuel Prede", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 530, "year": 1813, "date": "1813\/08", "State": "Conneticut", "Sublocation": "New Haven", "Location": "New Haven", "Lat": 41.3059, "Long": -72.920424, "Description": "August: Riot between American and foreign seamen (Swedish and Portuguese) on the docks. Broken up by the Governor's Guard armed with loaded muskets and bayonets. At foot of Long Wharf.", "Source": "Seymour.Ships, Sailors, and Samaratins, p.41", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; American; Swedish; Portuguese", "Target Classification": "Sailor; American; Swedish; Portuguese", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -72.920424, 41.3059 ] } },
{ "type": "Feature", "properties": { "ID": 531, "year": 1813, "date": "1813\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Some French gentlemen harassed (??) Beacham, beating her and threatening to destroy her house. They also claimed that they could raise enough Frenchmen to beat the watch which she said she would call to protect her.", "Source": "NYC General Sessions, September 15, 1813", "Type": "Ethnic\/National; Religious", "Rioter Classification": "French; Catholic", "Target Classification": "Woman", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Beacham", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 533, "year": 1813, "date": "1813\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: illiterate (at least he couldn't write) June Scott had his congregation harrassed by a number of persons. One of whom was identified and tried. Result uncertain. The mob kicked at the door tearing one of the staples out.", "Source": "NYC General Sessions, January 7, 1814", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "June Scott", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 539, "year": 1814, "date": "1814\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Complaint of \"sundry Inhabitants\" of the \"riotous conduct of boys assembling on Sundays at the African church corner of Leonard and Church street and of the extreme late hour of the night to which the exercises in said church are continued…\"", "Source": "MCC7:729", "Type": "Religious; Racial", "Rioter Classification": "Youth; Black; Religious", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 540, "year": 1814, "date": "1814\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: When the German Lutheran church announced that they would have one service in English as well certain people of the congregation were outraged, acted uproariously and were tried for riot. They were acquitted. Note the minister had been threatened", "Source": "NYC General Sessions, May 10, 1814", "Type": "Religious; Ethnic\/National", "Rioter Classification": "Religious; Lutheran; German", "Target Classification": "Religious; Lutheran; German; Minister", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 541, "year": 1814, "date": "1814\/05", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "May: Some Spanish sailors burst into a boarding house at the North end of town in search of some Greeks who had wounded a Spaniard the night before. Making a riot and tumult in the street they seized a Greek sailor, tired his hands behind him and drove him to Charleston. When near the yard some Greeks rushed by the sentry for protection. The Spaniards knocked down the guard and were immediately seized and put in (??) by Commodore Bawbridge. 26 were arrested and charged as riotous.", "Source": "N.Y.Gazette, May 29, 1814", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; Spanish; Catholic; Hispanic", "Target Classification": "Sailor; Greek; Orthodox", "Actions": "Seized Person(s); Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 542, "year": 1814, "date": "1814\/06", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Charleston", "Lat": 32.786536, "Long": -79.942879, "Description": "June: A Te Deum to be sung on Sunday at the request of the city's Roman Catholics offering thanks for the release of Pope Pius VII from prison brought the threat of mob violance by a number of indignant Frenchmen. With the promise of 150 to 200 of their countrymen to support them prompt action was taken by the Intendent who ordered out a strong guard. The ringleader was arrested in the body of the church, about to begin the disruption. that night some individuals threatened to burn the schooner Bourbon, bound for NY. It's name was obnoxious to a number of persons, particularly since the vessel carried a white flag with its name emblazoned on it. During the day an attempt was made to haul down the flag, but the crew prevented that from happening.", "Source": "N.Y.Gazette, July 9, 1814", "Type": "Religious; Ethnic\/National; Political", "Rioter Classification": "French", "Target Classification": "Religious; Catholic; Sailor", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -79.942879, 32.786536 ] } },
{ "type": "Feature", "properties": { "ID": 553, "year": 1815, "date": "1815\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Amos Broad petitions the Common Council complaining about riotous conduct at Rose street church.", "Source": "MCC 8:173", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 555, "year": 1815, "date": "1815\/08", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "August: Sunday - an Irish sailor led his company in flogging and tar and feathering an English sailor. The Englishman had been part of a press gang in England and had destroyed the Irishmen's protection.", "Source": "NYEP, October 2, 1815", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Sailor; Irish; Catholic", "Target Classification": "Sailor; British", "Actions": "Humiliation; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 556, "year": 1815, "date": "1815\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: John W. Jarvis, among others, disturbed Broad's church.", "Source": "SC PC - cases 7434", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "John W. Jarvis", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 566, "year": 1816, "date": "1816\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: Several black men were harrassed by, apparently, some Irish men on Bowster street. A white man was also attacked. Two were arrested and tried, one convicted.", "Source": "NYC General Sessions, June 11, 1816", "Type": "Racial; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Black; White", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 571, "year": 1816, "date": "1816\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Amos Broad's church was greatly disturbed and insulted by violent outrages committed. Crowds forced their way into the house and disturbed the society and minister.", "Source": "SC PC cases - 7434", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat; Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 572, "year": 1817, "date": "1817\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: Jacob Johnson, alias Stewart, harrassed Broad's rose street church. He made a nuisance of himself and gave a false report offense.", "Source": "SC PC - cases 7434 14", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": "Jacob Johnson", "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 576, "year": 1817, "date": "1817\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: Amos Broad's church was ransacked by a mob. Little details. All tried were acquitted.", "Source": "NYC General Sessions, February 5, 1817", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 578, "year": 1817, "date": "1817\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Members of the African Episcopal church petition the Common Council complaining of the riotous conduct at Broad's Rose Street church.", "Source": "MCC 9:40", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 580, "year": 1817, "date": "1817\/03", "State": "New York", "Sublocation": "Lansingburgh", "Location": "Lansingburgh, NY", "Lat": 42.731883, "Long": -73.686965, "Description": "March: A \"stuffed Paddy\" was paraded in this town, mocking the Irish and their patron saint.", "Source": "N.Y.Shamrock, March 29, 1817", "Type": "Ethnic\/National; Religious", "Rioter Classification": "American; Protestant", "Target Classification": "Irish; Catholic", "Actions": "Effigy", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.686965, 42.731883 ] } },
{ "type": "Feature", "properties": { "ID": 581, "year": 1817, "date": "1817\/03", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "March: Insulted by a British brig's flying a tricolor vane, local French citizens rioted. Armed with pistols and swords they ordered the Bitish captain to take down the vane. He refused. They attacked, killing the captain and four of his crew. 100 shots were fired. The masts were cut down. Gen. Ripley ordered out his men, arrested 15 rioters. The mayor ordered them released - the captains in part threatened to tar and feather the mayor. The 1st mate and 10 or 12 seamen were also taken into custody. The British consul declared himself and the others prisoners of war and the ship a prize. The next day every English ship in port flew the tricolor vane.", "Source": "N.Y.National Advertiser, April 17, 1817", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "French; Catholic", "Target Classification": "British; Sailor; Officer; Military", "Actions": "Lethal Violence", "Recorded # Killed": "5", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 582, "year": 1817, "date": "1817\/03", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "March: A British and French ship lie next to one another at the levee. On the 17th a dispute arose concerning the stage leading to the levee. It was limited and settled with the British officer bound to keep the peace. Thereafter it was noticed that the British ship had a tricolor tails on its vane. This was believed to be an insult to the N.O. French community. The next morning a crowd appeared and the mayor ordered the British officer to remove the vane. He did so, but in the afternoon, with an insurance from the US Army commander that the tails was not an insult to American pride, he hung it again. A mob rapidly formed and with the mayor present, who did nothing, took over the ship, cutting down the vane, rigging, chopping the main and mizzen masts, and firing into the cabin where the captain and British consul were . More damage would have been done had a detachment of the US infantry come up. 2 or 3 rioters were arrested, other were taken later as the mob dispersed.", "Source": "NYEP, April 23, 1817", "Type": "Ethnic\/National; Political; Religious", "Rioter Classification": "French; Catholic", "Target Classification": "British; Sailor; Officer; Military", "Actions": "Property Destroyed; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 585, "year": 1817, "date": "1817\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Broad's church was harassed again. A crowd of 300 to 500 gathered, made noise, broke the few lights over the church door, etc. Eight persons were arrested. The Gazette puts the number at a 1000 and tends to blame Broad for the disturbances.", "Source": "SC PC - cases 7434 6; N.Y.Gazette, April 25, 1817", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 586, "year": 1817, "date": "1817\/04", "State": "Louisiana", "Sublocation": "New Orleans", "Location": "New Orleans", "Lat": 29.944425, "Long": -90.08651, "Description": "April: Riot starts with argument between French and English captain.", "Source": "National Register III, p.270, April 25, 1817", "Type": "Ethnic\/National; Religious", "Rioter Classification": "French; Catholic; Sailor; Officer", "Target Classification": "Sailor; British; Officer", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -90.08651, 29.944425 ] } },
{ "type": "Feature", "properties": { "ID": 592, "year": 1817, "date": "1817\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Broad's church was attacked. Brick bats and stones thrown at it and one person got inside to harass Broad and the congregation.", "Source": "SC PC - cases 7434 10", "Type": "Religious; Slavery", "Rioter Classification": "Anti-Slavery", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 595, "year": 1818, "date": "1818\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: Broad's church was so frequently the scene of a riot, both within and without that the mayor called for a Grand Jury investigation in December. The result was a common law indictment of Broad for a nuisance. Sharp lawyers got a delay for Broad and the interim he at last abandoned his church, suspending services and promising to sell the building. The case was therefore dropped.", "Source": "N.Y. City-Hall recorder, January, 1818", "Type": "Religious; Slavery", "Rioter Classification": "Unknown", "Target Classification": "Religious; Baptist; Slaveholder", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": "Amos Broad", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 613, "year": 1819, "date": "1819\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: The African church in the 6th ward was disturbed by two men and unspecified others. The two men refused to leave until the watchmen came and took them away. They were after someone or some people in the chuch. Both convicted.", "Source": "NYC General Sessions, January 8, 1819", "Type": "Racial; Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Black; Church", "Actions": "Occupied Space; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Small", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 619, "year": 1819, "date": "1819\/03", "State": "Maryland", "Sublocation": "Baltimore", "Location": "Baltimore", "Lat": 39.293324, "Long": -76.615924, "Description": "March: About 2 o'clock, just as the merchants returned to dinner, a number of Irishmen rioted. They were enflamed by the \"Stuffed Paddy\" hung on the schooner Appomatax's top sail yard arm. They entered the ship, which had been seized by the custom officers, cut away the rigging and did other damage. The ship lay at the foot of Gray street at Smith's dock. Several gentlemen, who attempted to stop the proceedings, received some blows. This included one of the Judges of the city court. Eight or ten men were arrested for riot. The stuff paddy was hung by some boys as a prank.", "Source": "NYEP, March 20, 1819", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Judge", "Actions": "Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -76.615924, 39.293324 ] } },
{ "type": "Feature", "properties": { "ID": 624, "year": 1819, "date": "1819\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: The trustees of the Baptist Church in Mulberry street complain of the riotous conduct of people who harass their services on Sundays.", "Source": "MCC10: 368", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Baptist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 627, "year": 1819, "date": "1819\/06", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "June: A complaint of \"sundry inhabitants\" of \"inderent and disorderly conduct of certain black persons who assemble in (??) and Henry streets on Sundays and disturb persons attending Divine worship in the church in Bouker street.\" asn asking for a peace officer to be stationed there.", "Source": "MCC10:452", "Type": "Religious", "Rioter Classification": "Black", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 641, "year": 1820, "date": "1820\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: About noon Charles French, with a number of others, prevented the trustees of St. Peter's chuch from selling pews on the steps of the church. The crowd gathered is estimated at 200 persons.", "Source": "SC PC - cases 7436 3", "Type": "Religious", "Rioter Classification": "Religious", "Target Classification": "Religious", "Actions": "Threat; Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Local", "Persons Named as Rioters": "Charles French", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 657, "year": 1821, "date": "1821\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: In reponse to clergy profanation of Sabbath movement a great number of people assembled at an appointed place and voted resolves against the clergy intereference with local concerns of the city, or the police of the city. They felt it was improper of these clergy to do so and considered it \"an attempt to encroach upon the liberties of the citizen.\"", "Source": "Stokes.IconographyV, p.1677", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 661, "year": 1822, "date": "1822\/02", "State": "South Carolina", "Sublocation": "Columbia", "Location": "Columbia College, SC", "Lat": 34.006972, "Long": -81.039326, "Description": "February: Riot when junior class refused to attend prayers and recitations.", "Source": "Bruce.Violence and Culture, p.62-63", "Type": "Student; Religious", "Rioter Classification": "Student; Youth", "Target Classification": "University", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -81.039326, 34.006972 ] } },
{ "type": "Feature", "properties": { "ID": 664, "year": 1822, "date": "1822\/04", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "April: A dispute over the control of St. Mary's pastor between the Bishop and some of the congregation erupted into a large riot on April 8. The Catholic church divided over the dismissal of a liberal priest who wanted to have Sunday school and teach the youth from the bible. An election for the trustees was the immediate course of the confrontation which saw the Bishops men hold the church against the Hoganites (Hogan was the erring minister) until the civil authority was forced to intercede.", "Source": "Graphic Acct. of Riots at St. Mary's Church", "Type": "Religious", "Rioter Classification": "Religious; Catholic", "Target Classification": "Religious; Church; Catholic", "Actions": "Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 666, "year": 1822, "date": "1822\/04", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "April: Services at St. Mary's interrupted by some of those involved in the late disturbances. The authorities interfered to restore order.", "Source": "NYNational Advertiser, April 16, 1822", "Type": "Religious", "Rioter Classification": "Religious; Catholic", "Target Classification": "Religious; Church; Catholic", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 670, "year": 1822, "date": "1822\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: Attempt of black people loyal to an Allenite bishop to take over a splinter group's church on Lombard street between 5t and 6th streets. The Allenites were successful. But several arrests were made and men were bound over to the Mayor's court.", "Source": "NYAmerican, August 11, 1822", "Type": "Religious", "Rioter Classification": "Religious; Black; Allenite", "Target Classification": "Religious; Church", "Actions": "Occupied Space", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 671, "year": 1822, "date": "1822\/08", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "August: Robert R. Brown with 40 or so other disrupted a Baptist church off Christine and Delancy.", "Source": "NYC General Sessions, November 12, 1822", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Baptist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Unknown", "Persons Named as Rioters": "Robert R. Brown", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 673, "year": 1822, "date": "1822\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: About 100 sailors marched \"with drums, fifes, and colors, stopping at public places, and going to the city hall and huzzahing.\" Reason: A ship to be brought by the Columbian government had hired a smaller than anticipated interim crew at short wages. Eventually the civil authority was sent for; some of the ringleaders were taken. The rest dispersed peacably.", "Source": "NY.Statesman and Advertiser for the Country, September 16, 1822", "Type": "Labor; Ethnic\/National; Religious", "Rioter Classification": "Sailor; American", "Target Classification": "Columbian; Catholic", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 674, "year": 1822, "date": "1822\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Robert R. Brown disrupted the religious congregation at the church, corner of Delancy and (??) streets. When they attempted to apprehend Brown they were prevented as he was rescued by a crowd.", "Source": "SC PC - cases 7437 4", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Robert R. Brown", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 680, "year": 1823, "date": "1823\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: While attempting to take a young drunk disturber of their church the Baptists of the Brown St. church were set upon by a number of people attempting to rescue the culprit.", "Source": "SC PC - cases 7437 4", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Baptist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 686, "year": 1823, "date": "1823\/06", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "June: A yankee and Irishman quarrelled while working in paving a court. A fight ensued and a large crowd gathered. The crowd went to the Irishman's house, demolished the windows with stones and brick bats. One watchman, who tried to interfere, was slightly stabbed. It took several constables to restore order. One man was arrested.", "Source": "NYEP, June 24, 1823", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Worker; American", "Target Classification": "Irish; Catholic; Worker; Legal; Police; Constable", "Actions": "Non-Lethal Violence; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 698, "year": 1824, "date": "1824\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: A procession of Orangemen marched out of the city towards Greenwich village, then settled mostly by Irish Catholics. They had banners waving and a band playing \"croppies lie down\" and other equally provocative ditties. In the ensuing fight no one was killed but \"the orangemen received a sound thrashing and were driven into the city where upon the sheriff arrested every man he could find in Greenwich village. Over a hundred Irishmen were arraigned and charged with riot and disturbing the peace and were almost certain of conviction. In the September term of the court of sessions, the Judge was about to pass sentence when Thomas Addis Emett walked into the court room for another case and defended the Irishmen speaking loudly of the intolerance in the city.", "Source": "Murphy and Mannion.The Hist. of the Soc. Of The Friendly Sons of St. Pat., p.257", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Orangeman; Protestant", "Target Classification": "Irish; Catholic", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 699, "year": 1824, "date": "1824\/07", "State": "New York", "Sublocation": "Lockport", "Location": "Lockport, NY", "Lat": 43.172167, "Long": -78.695694, "Description": "July: On the anniversary of the Battle of the Boyne, Irish canal workers broke out into fighting among themselves usuing shereliahs and brickbats.", "Source": "NYNational Advertiser, July 23, 1824", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; Worker; Canal Worker", "Target Classification": "Irish; Catholic; Worker; Canal Worker", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -78.695694, 43.172167 ] } },
{ "type": "Feature", "properties": { "ID": 719, "year": 1825, "date": "1825\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: \"Irish Shillelah frolic\" took place in the morning near the marble buildings. Several hundred Irishmen were involved. Probably an Irish-Orange disturbance. It was \"Paddy up stairs\" vs \"Paddy downstairs\". It began over a disagreement about a raffle. There had been several clashes during the week. Constables Hays and Homan were there. Both were attacked and lucky to escape with their lives. About 10 ringleaders were arrested.", "Source": "NYAmerican, March 31, 1825", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; Protestant; Orangeman", "Target Classification": "Irish; Catholic; Protestant; Orangeman; Legal; Law Officer", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Hays; Homan", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 720, "year": 1825, "date": "1825\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Twelve Irishmen charged with rioting. Some fighting occurred on the 30th, but the big battle was on the 31st. The Irishmen fought the watch and constables as well.", "Source": "NYC General Sessions, April 7, 1825", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 721, "year": 1825, "date": "1825\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Irish and Yankee cartmen came to blows over the use of large cart (??). The Irish didn't like the yankee using them. Stones were thrown and blows exchanged. Might just be an A+B.", "Source": "NYAmerican, May 9, 1825", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Irish; Catholic; American; Protestant; Worker; Cartman", "Target Classification": "Irish; Catholic; American; Protestant; Worker; Cartman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 727, "year": 1825, "date": "1825\/06", "State": "Kentucky", "Sublocation": "Harrodsburg", "Location": "Harrodsburg, KY", "Lat": 37.762949, "Long": -84.846385, "Description": "June: A shakertown nearby was harassesd by 30 or 40 men who wanted to take a woman out of the society. Both the woman and her mother had been shakers. Her mother, however, voluntarily left, and thought the daughter should leave too. It is unclear whether the daughter wanted to stay or go.", "Source": "NYEP, June 30, 1825", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Shaker; Religious; Woman", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Medium", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -84.846385, 37.762949 ] } },
{ "type": "Feature", "properties": { "ID": 732, "year": 1825, "date": "1825\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: Orange-Irish riot on the wharfs between race and vine streets. 100 to 200 persons involved. The mayor and the police quelled the disturbance. Several ringleaders arrested. The cause was \"European politics,\" while on board, emigrating to America, Orangemen celebrated July, the anniversary of the Battle of the Boyne. The ribbonmen on board objected, but in the ensuing battle they were outnumbered and lost. But when they landed in Philly, their numbers were augmented.", "Source": "NYEP, August 3, 1825", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic; Protestant; Orangeman; Ribbonman", "Target Classification": "Irish; Catholic; Protestant; Orangeman; Ribbonman", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 733, "year": 1825, "date": "1825\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: At Harlem, P. Samler had his house surrounded \"by a number of men collected together to tear down his house…and his house was stoned and the door broken open and part of the roof torn off by them and they entered and both him and his wife…were beaten and a declaration was made that no de-Irishman should live on the commons...\"", "Source": "SC PC 7438 4", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Nativist; American", "Target Classification": "Irish; Catholic; Woman", "Actions": "Threat; Property Destroyed; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 2, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": "P. Samler", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 739, "year": 1825, "date": "1825\/12", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "December: A cry of murder was raised from a house in Broad street by Irish immigrants. When entering, one of two watchmen was struck with an ax. An Irishman was arrested. The Gazette says that there was a riot among the Irish on Broad street.", "Source": "NYAmerican, December 15, 1825", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Police; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 748, "year": 1826, "date": "1826\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: Irish cartman beat up on James Anderson because they suspected that he was not a Roman Catholic and because he worked too hard on a job with them. A+B case.", "Source": "NYC General Sessions, April 7, 1826", "Type": "Ethnic\/National; Religious; Labor", "Rioter Classification": "Irish; Catholic; Worker; Cartman", "Target Classification": "Worker; Cartman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": 1, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "James Anderson", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 749, "year": 1826, "date": "1826\/03", "State": "District of Columbia", "Sublocation": "Washington", "Location": "Washington DC", "Lat": 38.882651, "Long": -77.013549, "Description": "March: Rioters broke several lamps and windows belonging to the corporation, broke windows at the French Minister's and carried off his lamps.", "Source": "NYGazette, April 8, 1826", "Type": "Political; Ethnic\/National; Religious", "Rioter Classification": "Unknown", "Target Classification": "French; Catholic; Diplomat; City Corporation", "Actions": "Property Destroyed; Seized Property", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "National", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.013549, 38.882651 ] } },
{ "type": "Feature", "properties": { "ID": 751, "year": 1826, "date": "1826\/07", "State": "Massachusetts", "Sublocation": "Boston", "Location": "Boston", "Lat": 42.350273, "Long": -71.060098, "Description": "July: Large scale riots among Irish. It began as an argument between some coopers' boys and some Irishmen over how the latter carelessly handled some barrels. The riots began on Tuesday afternoon and were not quelled until Friday. The disturbances ranged from the mill-pond to Broad street. Several ringleaders were arrested. Many could not raise bail and were committed until the August Municipal court.", "Source": "NYGazette, July 20, 1826", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Unknown", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -71.060098, 42.350273 ] } },
{ "type": "Feature", "properties": { "ID": 752, "year": 1826, "date": "1826\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: A number of apparent Irishmen rioted in Pearl street opposing Marshalls and resisting arrest. It started with some Irishmen attacking a peaceful passerby.", "Source": "NYGazette, July 7, 1826", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Law Officer; Marshal", "Actions": "Unknown", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 774, "year": 1827, "date": "1827\/01", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "January: A large riot which saw Irish and watchmen come to blows. Several watchmen were badly hurt - two of their cops were beat in pieces. It apparently started with an Irish attack on a black man while he was walking in Anthony street. All but ten of the rioters were convicted, including Patrick Winters.", "Source": "NYC General Sessions, January 3, 1827", "Type": "Racial; Law; Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Black; Legal; Law Officer; Watchman", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Patrick Winters", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 779, "year": 1827, "date": "1827\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: A young man was disorderly in the Chrystie street church. When an attempt was made to put him out he called other in and commenced a fight.", "Source": "SC PC cases 7439 12", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Church; Religious", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 783, "year": 1827, "date": "1827\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: The Union African Church was disturbed at 8 o'clock. A stone was thrown, a mob formed and services interrupted.", "Source": "SC PC cases 7439 8", "Type": "Racial; Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Black; Church; Union African", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 809, "year": 1828, "date": "1828\/02", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "February: A riot occurred Sunday afternoon in a vacant lot between Broome and Grand streets, where a number of persons had collected to hear the preaching of John Edwards. Stones and brick bats flew about.", "Source": "N.Y.National Advertiser, Feb. 13, 1828", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Unknown", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 817, "year": 1828, "date": "1828\/03", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "March: A Greenwich Irish laborer died of tetanus. The attending doctor asked the landlord, who had care of the man and his belongings, if he could do an autopsy. He said okay…while the doctor and colleage were dissecting the deceased, a \"robust countryman of the deceased\" came and started to argue with them. He left and returned with a mob which threatened violence unless the doctors ceased their barbarous handling of their countryman. Luckily the doctors escaped unscathed. But the 1st doctor was harassed for several days thereafter.", "Source": "Statesman, April 4, 1828", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Doctor", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 819, "year": 1828, "date": "1828\/04", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "April: Thirteen men indicted for riot. A house, which formerly belonged to RG Stuyvesant, on 1st ave was stoned and almost broken into by a large crowd of 200-500 persons. There was talk of having the satisfaction of the Irish. The watch was called to quell the disturbance and were resisted. The house opposite to Stuyvesant's was partially demolish and fences were likewise torn down.", "Source": "NYC General Sessions, April 11, 1828", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Law Officer", "Actions": "Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "RG Stuyvesant", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 844, "year": 1828, "date": "1828\/08", "State": "New York", "Sublocation": "Hudson River", "Location": "Hudson River", "Lat": 42.570786, "Long": -73.756873, "Description": "August: Steamboat Henry Eckford had its passage from NYC to Albany disturbed by a riot. The steamboat carried in tow a number of freight barges which also carried passages. A dispute between the captain and two Irishmen over the amount charged to a female passenger led to a riot as the barge was cut adrift by the riotors and an attempt was made to board the steamboat. Several of the rioters were arrested.", "Source": "NYNational Advertiser, August 5, 1828", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Sailor; Captain; Officer", "Actions": "Non-Lethal Violence; Threat; Property Destroyed", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.756873, 42.570786 ] } },
{ "type": "Feature", "properties": { "ID": 845, "year": 1828, "date": "1828\/08", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "August: Kensing weavers riot. Stephen Heimer, a watchman, was killed. Heimer was off duty. He entered an Irish tavern, made noise as he had in the street, 3rd street above Poplar Lowe, and called the owners \"bloody Irish transports.\" Hence he was attacked. He had been asked to be quiet because there was a dying woman in the house. Irish weavers riot, leaving one watchman dead. The rioters seized a building and hoisted a flag of the weaver's trade.", "Source": "Scharf and Wescott, I, p.623", "Type": "Ethnic\/National; Religious; Law; Labor", "Rioter Classification": "Irish; Catholic; Worker; Weaver", "Target Classification": "Legal; Police; Watchman", "Actions": "Lethal Violence", "Recorded # Killed": "1", "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Stephen Heimer", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 846, "year": 1828, "date": "1828\/09", "State": "New Jersey", "Sublocation": "Glamster Point", "Location": "Glamster Point, NJ", "Lat": 39.972164, "Long": -75.044147, "Description": "August: A camp (?) meeting of black people, which was held for several days, was broken up by white people. This was near Philadelphia.", "Source": "NYStatesman, August 23, 1828", "Type": "Racial; Religious", "Rioter Classification": "White", "Target Classification": "Black; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": "PTH Edits", "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.044147, 39.972164 ] } },
{ "type": "Feature", "properties": { "ID": 851, "year": 1828, "date": "1828\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: Thomas Chick, a decent looking fellow, was taken up for promoting a riot in front of the Methodist church in Forsythe street. The chuch did not want him fined or imprisoned but only bound to keep the peace. He was fined $25 or put security up to keep the peace.", "Source": "NYGazette, September 20, 1828", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Thomas Chick", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 860, "year": 1828, "date": "1828\/11", "State": "Massachusetts", "Sublocation": "Salem", "Location": "Salem, MA", "Lat": 42.521367, "Long": -70.895567, "Description": "November: Five boys arrested for disturbing the congregation of the Tabernacle church on Sunday evening.", "Source": "NYGazette, November 15, 1828", "Type": "Religious", "Rioter Classification": "Youth", "Target Classification": "Religious; Church; Tabernacle", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -70.895567, 42.521367 ] } },
{ "type": "Feature", "properties": { "ID": 886, "year": 1829, "date": "1829\/05", "State": "Pennsylvania", "Sublocation": "Clark's Ferry", "Location": "Clark's Ferry, Pa", "Lat": 40.411313, "Long": -77.00939, "Description": "May: An Irishman refused to pay for his whiskey at a local saloon. After being kicked out he returned with a number of his countrymen to pull down the building and kill the innkeeper Martin. Finding the house secure they began pulling it down. Martin threatened to shoot them, and finally, in desperation, commenced firing. No one was killed, but several were wounded. The sheriff ordered out the posse to restore order.", "Source": "NYEP, May 9, 1829", "Type": "Ethnic\/National; Religious", "Rioter Classification": "Irish; Catholic", "Target Classification": "Merchant", "Actions": "Property Destroyed; Seized Property; Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": "Martin", "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -77.00939, 40.411313 ] } },
{ "type": "Feature", "properties": { "ID": 890, "year": 1829, "date": "1829\/05", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "May: When two officers entered a house in Augustus street to levy an execution on the goods of an Irishman, \"a general turn out of his countrymen took place.\" A riot was prevented by the timely arrival of the police. The goods were removed and the officers beat a hasty retreat.", "Source": "New York Statesman, June 2, 1829", "Type": "Ethnic\/National; Religious; Law", "Rioter Classification": "Irish; Catholic", "Target Classification": "Legal; Law Officer", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 892, "year": 1829, "date": "1829\/07", "State": "Pennsylvania", "Sublocation": "Erie", "Location": "Erie, Penn", "Lat": 42.133234, "Long": -80.084094, "Description": "July: When a masonic methodist preacher attempted to preach at a church in Erie, he was disrupted and forced to continue in a nearby barn.", "Source": "NYEP, July 24, 1829", "Type": "Religious; Anti-Masonic", "Rioter Classification": "Anti-Masonic", "Target Classification": "Religious; Methodist; Church; Masonic", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -80.084094, 42.133234 ] } },
{ "type": "Feature", "properties": { "ID": 893, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: The African church on 6th avenue was distubed during services by a number of boys - one of whom wore his jacked inside out. Stones were thrown and a cart ring was threatened to be ued in hitting someone. This kind of activity occurred frequently. Timothy Duffy and Michael Tweeny were indicted and convicted of riot and A+B.", "Source": "NYC General Sessions, July 14, 1829", "Type": "Racial; Religious", "Rioter Classification": "Youth", "Target Classification": "Black; Church; Religious", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Timothy Duffy; Michael Tweeny", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 896, "year": 1829, "date": "1829\/07", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "July: Religious assembly held at Christie street between Hester and Walker was disrupted by Jacob C. Cauldwell and others. Cauldwell plead guilty to riot and A+B.", "Source": "NYC General Sessions, July 16, 1829", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Jacob C. Cauldwell", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 906, "year": 1829, "date": "1829\/09", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "September: The Ebenezer church in Elizabeth street, probably for black people, was disrupted during divine services. Some scuffle ensued. Two white men were in the church while more were outside.", "Source": "NYC General Sessions, October 13, 1829", "Type": "Racial; Religious", "Rioter Classification": "White", "Target Classification": "Black; Church; Religious", "Actions": "Threat; Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 909, "year": 1829, "date": "1829\/10", "State": "New York", "Sublocation": "Troy", "Location": "Troy, NY", "Lat": 42.731883, "Long": -73.686965, "Description": "October: In the lower part of the city riot erupted when some Irish attempted to have a dance at a house. Neighbors through that this was not right and threw some stones at the windows. Duley insulted, the Irish counter-attacked with fist, clubs, and the like. Nearly 200 were engaged in the tumult. The police arrived, and with difficulty arrested 3 or 4 ringleaders and quelled the riot.", "Source": "NYEP, October 30, 1829", "Type": "Ethnic\/National; Religious", "Rioter Classification": "American; Irish; Catholic", "Target Classification": "American; Irish; Catholic", "Actions": "Non-Lethal Violence; Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Large", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -73.686965, 42.731883 ] } },
{ "type": "Feature", "properties": { "ID": 914, "year": 1829, "date": "1829\/11", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "November: Eliah Archer, with others disturbed the methodist meeting house near 3rd avenue during public worship. Archer has done this before. Result: convicted.", "Source": "NYC General Sessions, December 14, 1829", "Type": "Religious", "Rioter Classification": "Unknown", "Target Classification": "Religious; Methodist; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": "Eliah Archer", "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
{ "type": "Feature", "properties": { "ID": 915, "year": 1829, "date": "1829\/11", "State": "Pennsylvania", "Sublocation": "Philadelphia", "Location": "Philadelphia", "Lat": 39.90153, "Long": -75.176711, "Description": "November: White and black people fought in Cedar Ward following a loud and emotional religious service.", "Source": "Nash.Forging Freedom, p.275", "Type": "Racial; Religious", "Rioter Classification": "White; Black", "Target Classification": "White; Black", "Actions": "Non-Lethal Violence", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Unknown", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -75.176711, 39.90153 ] } },
{ "type": "Feature", "properties": { "ID": 919, "year": 1829, "date": "1829\/12", "State": "New York", "Sublocation": "New York City", "Location": "NYC", "Lat": 40.720368, "Long": -74.001098, "Description": "December: Bedford St. church was often harassed by groups of young boys who come there, run in and out and laugh, hallow and use indecent language. This occurred on Sunday, the 12th towards the end of the services.", "Source": "SC PC cases 7441 9", "Type": "Religious", "Rioter Classification": "Youth", "Target Classification": "Religious; Church", "Actions": "Threat", "Recorded # Killed": null, "Recorded # Injured": null, "Riot Size": "Unknown", "Riot Scope": "Local", "Persons Named as Rioters": null, "Persons Named as Targets": null, "Note": null, "field_22": null }, "geometry": { "type": "Point", "coordinates": [ -74.001098, 40.720368 ] } },
